            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2015/14/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "House of Lords (Expulsion and Suspension) Act 2015",
                    "tokenCum": 62,
                    "wordSum": 8,
                    "tokenSum": 19,
                    "tokenCount": 18
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2015 CHAPTER 14",
                    "tokenCum": 71,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision empowering the House of Lords to expel or suspendmembers.",
                            "tokenCum": 92,
                            "wordSum": 14,
                            "tokenSum": 22,
                            "tokenCount": 22
                        }
                    ],
                    "wordSum": 17,
                    "tokenSum": 32,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[26th March 2015]",
                    "tokenCum": 103,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 160,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Expulsion and suspension of members of the House of Lords",
                            "tokenCum": 173,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "Standing Orders of the House of Lords may make provision under which theHouse of Lords may by resolution\u2014",
                                    "tokenCum": 198,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "expel a member of the House of Lords, or",
                                            "tokenCum": 210,
                                            "wordSum": 9,
                                            "tokenSum": 15,
                                            "tokenCount": 15
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "suspend a member of the House of Lords for the period specified in theresolution.",
                                            "tokenCum": 230,
                                            "wordSum": 14,
                                            "tokenSum": 23,
                                            "tokenCount": 23
                                        }
                                    ],
                                    "wordSum": 41,
                                    "tokenSum": 65,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "A person expelled by virtue of this section ceases to be a member.",
                                    "tokenCum": 246,
                                    "wordSum": 13,
                                    "tokenSum": 18,
                                    "tokenCount": 18
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "A person suspended by virtue of this section remains a member during theperiod of suspension, but during that period the person\u2014",
                                    "tokenCum": 272,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "is not entitled to receive writs of summons to attend the House of Lords,and",
                                            "tokenCum": 291,
                                            "wordSum": 14,
                                            "tokenSum": 22,
                                            "tokenCount": 22
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "despite any writ of summons previously issued to the person, isdisqualified from sitting or voting in the House of Lords or a committee ofthe House of Lords.",
                                            "tokenCum": 327,
                                            "wordSum": 27,
                                            "tokenSum": 39,
                                            "tokenCount": 39
                                        }
                                    ],
                                    "wordSum": 62,
                                    "tokenSum": 89,
                                    "tokenCount": 27
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "A resolution passed by virtue of subsection (1) must state that, in theopinion of the House of Lords, the conduct giving rise to the resolution\u2014",
                                    "tokenCum": 362,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "occurred after the coming into force of this Act, or",
                                            "tokenCum": 373,
                                            "wordSum": 10,
                                            "tokenSum": 14,
                                            "tokenCount": 14
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "occurred before the coming into force of this Act and was not publicknowledge before that time.",
                                            "tokenCum": 392,
                                            "wordSum": 16,
                                            "tokenSum": 22,
                                            "tokenCount": 22
                                        }
                                    ],
                                    "wordSum": 51,
                                    "tokenSum": 73,
                                    "tokenCount": 36
                                }
                            ],
                            "wordSum": 177,
                            "tokenSum": 259,
                            "tokenCount": 13
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Entitlement to receive writs of summons to attend House of Lords",
                            "tokenCum": 408,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "In section 6 of the House of Lords Reform Act 2014 (interpretation), aftersubsection (2)(c) insert\u2014",
                                    "tokenCum": 439,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "\u201c(d)",
                                            "text": "section 1(3) of the House of Lords (Expulsion and Suspension) Act 2015(suspension).\u201d",
                                            "tokenCum": 472,
                                            "wordSum": 12,
                                            "tokenSum": 36,
                                            "tokenCount": 36
                                        }
                                    ],
                                    "wordSum": 27,
                                    "tokenSum": 69,
                                    "tokenCount": 32
                                }
                            ],
                            "wordSum": 38,
                            "tokenSum": 86,
                            "tokenCount": 16
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Effect of ceasing to be a member",
                            "tokenCum": 480,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "A person expelled in accordance with section 1 is to be treated as if thatperson had ceased to be a member in accordance with the House of Lords ReformAct 2014, for the purposes of section 4(2) to (8) of that Act.",
                                    "tokenCum": 540,
                                    "wordSum": 41,
                                    "tokenSum": 62,
                                    "tokenCount": 62
                                }
                            ],
                            "wordSum": 48,
                            "tokenSum": 71,
                            "tokenCount": 8
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 4",
                            "text": "Short title, commencement and extent",
                            "tokenCum": 548,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act may be cited as the House of Lords (Expulsion and Suspension) Act2015.",
                                    "tokenCum": 574,
                                    "wordSum": 14,
                                    "tokenSum": 28,
                                    "tokenCount": 28
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act shall come into force at the end of the period of three monthsbeginning with the day on which this Act is passed.",
                                    "tokenCum": 601,
                                    "wordSum": 24,
                                    "tokenSum": 29,
                                    "tokenCount": 29
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This Act extends to England and Wales, Scotland and Northern Ireland.",
                                    "tokenCum": 614,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 614,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 11,
                                    "tokenSum": 18,
                                    "tokenCount": 14
                                }
                            ],
                            "wordSum": 54,
                            "tokenSum": 84,
                            "tokenCount": 8
                        }
                    ],
