            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2001/23/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Human Reproductive Cloning Act 2001",
                    "tokenCum": 56,
                    "wordSum": 5,
                    "tokenSum": 13,
                    "tokenCount": 12
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2001 CHAPTER 23",
                    "tokenCum": 65,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to prohibit the placing in a woman of a human embryo which has beencreated otherwise than by fertilisation.",
                            "tokenCum": 92,
                            "wordSum": 20,
                            "tokenSum": 28,
                            "tokenCount": 28
                        }
                    ],
                    "wordSum": 23,
                    "tokenSum": 38,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[4th December 2001]",
                    "tokenCum": 102,
                    "wordSum": 3,
                    "tokenSum": 11,
                    "tokenCount": 10
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "BE IT ENACTED by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 160,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "The Offence",
                            "tokenCum": 163,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "A person who places in a woman a human embryo which has been createdotherwise than by fertilisation is guilty of an offence.",
                                    "tokenCum": 193,
                                    "wordSum": 22,
                                    "tokenSum": 32,
                                    "tokenCount": 32
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "A person who is guilty of the offence is liable on conviction on indictmentto imprisonment for a term not exceeding 10 years or a fine or both.",
                                    "tokenCum": 231,
                                    "wordSum": 27,
                                    "tokenSum": 40,
                                    "tokenCount": 40
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "No proceedings for the offence may be instituted\u2014",
                                    "tokenCum": 243,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "in England and Wales, except with the consent of the Director of PublicProsecutions;",
                                            "tokenCum": 261,
                                            "wordSum": 13,
                                            "tokenSum": 21,
                                            "tokenCount": 21
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "in Northern Ireland, except with the consent of the Director of PublicProsecutions for Northern Ireland.",
                                            "tokenCum": 281,
                                            "wordSum": 15,
                                            "tokenSum": 23,
                                            "tokenCount": 23
                                        }
                                    ],
                                    "wordSum": 36,
                                    "tokenSum": 58,
                                    "tokenCount": 13
                                }
                            ],
                            "wordSum": 87,
                            "tokenSum": 134,
                            "tokenCount": 3
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Short title and extent",
                            "tokenCum": 285,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act may be cited as the Human Reproductive Cloning Act 2001.",
                                    "tokenCum": 306,
                                    "wordSum": 12,
                                    "tokenSum": 23,
                                    "tokenCount": 23
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act extends to Northern Ireland.",
                                    "tokenCum": 313,
                                    "wordSum": 6,
                                    "tokenSum": 9,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "Her Majesty may by Order in Council direct that any of the provisions ofthis Act are to extend, with such exceptions, adaptations and modifications(if any) as may be specified in the Order, to any of the Channel Islands.",
                                    "tokenCum": 361,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 361,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 38,
                                    "tokenSum": 53,
                                    "tokenCount": 49
                                }
                            ],
                            "wordSum": 60,
                            "tokenSum": 90,
                            "tokenCount": 4
                        }
                    ],
                    "wordSum": 183,
                    "tokenSum": 283,
                    "tokenCount": 58
                }
