    "children": [
        {
            "type": "Text",
            "prefix": "",
            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2012/16/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Infrastructure (Financial Assistance) Act 2012",
                    "tokenCum": 60,
                    "wordSum": 5,
                    "tokenSum": 17,
                    "tokenCount": 16
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2012 CHAPTER 16",
                    "tokenCum": 69,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision in connection with the giving of financial assistancein respect of the provision of infrastructure.",
                            "tokenCum": 91,
                            "wordSum": 19,
                            "tokenSum": 23,
                            "tokenCount": 23
                        }
                    ],
                    "wordSum": 22,
                    "tokenSum": 33,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[31st October 2012]",
                    "tokenCum": 102,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 159,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Expenditure on financial assistance for the provision of infrastructure",
                            "tokenCum": 171,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "There may be paid out of money provided by Parliament expenditure incurred\u2014",
                                    "tokenCum": 188,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "by the Treasury, or",
                                            "tokenCum": 195,
                                            "wordSum": 4,
                                            "tokenSum": 10,
                                            "tokenCount": 10
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "by the Secretary of State with the consent of the Treasury,",
                                            "tokenCum": 209,
                                            "children": [
                                                {
                                                    "type": "Text",
                                                    "prefix": "",
                                                    "text": "in giving, or in connection with giving, financial assistance to any person inrespect of the provision of infrastructure.",
                                                    "tokenCum": 233,
                                                    "wordSum": 18,
                                                    "tokenSum": 28,
                                                    "tokenCount": 28
                                                }
                                            ],
                                            "wordSum": 29,
                                            "tokenSum": 45,
                                            "tokenCount": 16
                                        }
                                    ],
                                    "wordSum": 45,
                                    "tokenSum": 74,
                                    "tokenCount": 18
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "\u201cInfrastructure\u201d includes\u2014",
                                    "tokenCum": 240,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "water, electricity, gas, telecommunications, sewerage or other services,",
                                            "tokenCum": 258,
                                            "wordSum": 8,
                                            "tokenSum": 21,
                                            "tokenCount": 21
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "railway facilities (including rolling stock), roads or other transportfacilities,",
                                            "tokenCum": 272,
                                            "wordSum": 9,
                                            "tokenSum": 17,
                                            "tokenCount": 17
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "health or educational facilities,",
                                            "tokenCum": 277,
                                            "wordSum": 4,
                                            "tokenSum": 8,
                                            "tokenCount": 8
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(d)",
                                            "text": "court or prison facilities, and",
                                            "tokenCum": 283,
                                            "wordSum": 5,
                                            "tokenSum": 9,
                                            "tokenCount": 9
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(e)",
                                            "text": "housing. ",
                                            "tokenCum": 286,
                                            "wordSum": 1,
                                            "tokenSum": 6,
                                            "tokenCount": 6
                                        }
                                    ],
                                    "wordSum": 29,
                                    "tokenSum": 70,
                                    "tokenCount": 8
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "\u201cProvision\u201d includes acquisition, design, construction, conversion,improvement, operation and repair.",
                                    "tokenCum": 308,
                                    "wordSum": 9,
                                    "tokenSum": 24,
                                    "tokenCount": 24
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "In this Act \u201cfinancial assistance\u201d means loans, guarantees or indemnities,or any other kind of financial assistance (actual or contingent).",
                                    "tokenCum": 343,
                                    "wordSum": 19,
                                    "tokenSum": 37,
                                    "tokenCount": 37
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "Expenditure which could be paid out of money provided by Parliament undersubsection (1) is to be charged on and paid out of the Consolidated Fund if\u2014",
                                    "tokenCum": 378,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the expenditure is incurred under an agreement entered into by the Treasuryor the Secretary of State, and",
                                            "tokenCum": 403,
                                            "wordSum": 17,
                                            "tokenSum": 28,
                                            "tokenCount": 28
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "the Treasury are satisfied that arrangements cannot reasonably be made forthe expenditure to be paid out of money provided by Parliament.",
                                            "tokenCum": 432,
                                            "wordSum": 21,
                                            "tokenSum": 32,
                                            "tokenCount": 32
                                        }
                                    ],
                                    "wordSum": 64,
                                    "tokenSum": 97,
                                    "tokenCount": 36
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "Where money is paid in reliance on subsection (5), the Treasury must assoon as is reasonably practicable lay a report before Parliament specifyingthe amount paid.",
                                    "tokenCum": 470,
                                    "wordSum": 25,
                                    "tokenSum": 40,
                                    "tokenCount": 40
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(7)",
                                    "text": "References in this Act to infrastructure assistance are to financialassistance the giving of which is in reliance on the authority provided bythis section.",
                                    "tokenCum": 499,
                                    "wordSum": 23,
                                    "tokenSum": 31,
                                    "tokenCount": 31
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(8)",
                                    "text": "Nothing in this Act applies in relation to the giving of financialassistance under a statutory power.",
                                    "tokenCum": 520,
                                    "wordSum": 16,
                                    "tokenSum": 23,
                                    "tokenCount": 23
                                }
                            ],
                            "wordSum": 239,
                            "tokenSum": 409,
                            "tokenCount": 12
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Limit on expenditure and liabilities",
                            "tokenCum": 529,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The amount of the Government\u2019s expenditure and liabilities under this Actmust not at any time exceed \u00a350,000 million.",
                                    "tokenCum": 560,
                                    "wordSum": 18,
                                    "tokenSum": 33,
                                    "tokenCount": 33
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "For this purpose the amount of the Government\u2019s expenditure and liabilitiesunder this Act is the aggregate of\u2014",
                                    "tokenCum": 584,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the expenditure incurred by the Treasury or the Secretary of State ingiving, or in connection with giving, infrastructure assistance, and",
                                            "tokenCum": 615,
                                            "wordSum": 20,
                                            "tokenSum": 34,
                                            "tokenCount": 34
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "the current amount of the actual or contingent liabilities of the Treasuryor the Secretary of State in respect of infrastructure assistance,",
                                            "tokenCum": 644,
                                            "children": [
                                                {
                                                    "type": "Text",
                                                    "prefix": "",
                                                    "text": "less any sums received by the Treasury or the Secretary of State in connectionwith infrastructure assistance.",
                                                    "tokenCum": 665,
                                                    "wordSum": 16,
                                                    "tokenSum": 25,
                                                    "tokenCount": 25
                                                }
                                            ],
                                            "wordSum": 37,
                                            "tokenSum": 57,
                                            "tokenCount": 31
                                        }
                                    ],
                                    "wordSum": 74,
                                    "tokenSum": 117,
                                    "tokenCount": 25
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "Where any expenditure, liabilities or sums mentioned in subsection (2) arein currencies other than sterling, their amount in sterling is to bedetermined for the purposes of subsection (1) at what appears to the Treasuryto be the appropriate rate of exchange.",
                                    "tokenCum": 727,
                                    "wordSum": 40,
                                    "tokenSum": 64,
                                    "tokenCount": 64
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "The Treasury may by order made by statutory instrument substitute a greateramount for the amount for the time being specified in subsection (1).",
                                    "tokenCum": 758,
                                    "wordSum": 23,
                                    "tokenSum": 33,
                                    "tokenCount": 33
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "A statutory instrument containing an order under subsection (4) may not bemade unless a draft of the instrument has been laid before and approved by aresolution of the House of Commons.",
                                    "tokenCum": 799,
                                    "wordSum": 31,
                                    "tokenSum": 43,
                                    "tokenCount": 43
                                }
                            ],
                            "wordSum": 191,
                            "tokenSum": 300,
                            "tokenCount": 9
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Reports ",
                            "tokenCum": 802,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The Treasury must, in relation to each relevant period\u2014",
                                    "tokenCum": 815,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "prepare a report in accordance with this section, and",
                                            "tokenCum": 826,
                                            "wordSum": 9,
                                            "tokenSum": 14,
                                            "tokenCount": 14
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "lay it before the House of Commons as soon as is reasonably practicableafter the end of that period.",
                                            "tokenCum": 849,
                                            "wordSum": 18,
                                            "tokenSum": 26,
                                            "tokenCount": 26
                                        }
                                    ],
                                    "wordSum": 36,
                                    "tokenSum": 55,
                                    "tokenCount": 14
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "\u201cRelevant period\u201d means\u2014",
                                    "tokenCum": 856,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the period beginning with the day on which this Act is passed and endingwith 31 March 2013, and",
                                            "tokenCum": 882,
                                            "wordSum": 18,
                                            "tokenSum": 29,
                                            "tokenCount": 29
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "each subsequent period of 12 months.",
                                            "tokenCum": 891,
                                            "wordSum": 6,
                                            "tokenSum": 12,
                                            "tokenCount": 12
                                        }
                                    ],
                                    "wordSum": 27,
                                    "tokenSum": 50,
                                    "tokenCount": 8
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "Each report must provide details of\u2014",
                                    "tokenCum": 898,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the arrangements entered into by the Treasury or the Secretary of Stateduring the relevant period for giving, or in connection with giving,infrastructure assistance,",
                                            "tokenCum": 932,
                                            "wordSum": 23,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "the expenditure incurred by the Treasury or the Secretary of State duringthat period in giving, or in connection with giving, infrastructureassistance,",
                                            "tokenCum": 966,
                                            "wordSum": 21,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "the amount of the actual or contingent liabilities of the Treasury or theSecretary of State at the end of that period in respect of infrastructureassistance,",
                                            "tokenCum": 1002,
                                            "wordSum": 25,
                                            "tokenSum": 39,
                                            "tokenCount": 39
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(d)",
                                            "text": "the sums received during that period by the Treasury or the Secretary ofState in connection with infrastructure assistance, and",
                                            "tokenCum": 1026,
                                            "wordSum": 19,
                                            "tokenSum": 27,
                                            "tokenCount": 27
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(e)",
                                            "text": "the amount of the Government\u2019s expenditure and liabilities under this Act,determined as at the end of that period in accordance with section 2.",
                                            "tokenCum": 1061,
                                            "wordSum": 23,
                                            "tokenSum": 38,
                                            "tokenCount": 38
                                        }
                                    ],
                                    "wordSum": 117,
                                    "tokenSum": 187,
                                    "tokenCount": 8
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "The report in relation to the first relevant period must also includedetails of the arrangements described in subsection (3)(a) entered into by theTreasury or the Secretary of State before the beginning of that period inanticipation of the coming into force of section 1.",
                                    "tokenCum": 1120,
                                    "wordSum": 43,
                                    "tokenSum": 61,
                                    "tokenCount": 61
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "No report is required in relation to a relevant period if there is nothingto record under subsections (3) and (4).",
                                    "tokenCum": 1146,
                                    "wordSum": 20,
                                    "tokenSum": 28,
                                    "tokenCount": 28
                                }
                            ],
                            "wordSum": 244,
                            "tokenSum": 385,
                            "tokenCount": 3
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 4",
                            "text": "Short title, commencement and extent",
                            "tokenCum": 1154,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act may be cited as the Infrastructure (Financial Assistance) Act2012.",
                                    "tokenCum": 1178,
                                    "wordSum": 11,
                                    "tokenSum": 26,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act comes into force on the day on which it is passed.",
                                    "tokenCum": 1192,
                                    "wordSum": 13,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This Act extends to England and Wales, Scotland and Northern Ireland.",
                                    "tokenCum": 1205,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 1205,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 11,
                                    "tokenSum": 18,
                                    "tokenCount": 14
                                }
                            ],
                            "wordSum": 40,
                            "tokenSum": 69,
                            "tokenCount": 8
                        }
                    ],
                    "wordSum": 750,
                    "tokenSum": 1221,
                    "tokenCount": 57
                }
            ],
            "wordSum": 781,
            "tokenSum": 1328,
            "tokenCount": 44
        }
    ],
