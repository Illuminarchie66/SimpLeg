            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2013/11/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Prisons (Property) Act 2013",
                    "tokenCum": 55,
                    "wordSum": 4,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2013 CHAPTER 11",
                    "tokenCum": 64,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision for the destruction of certain property found inprisons and similar institutions.",
                            "tokenCum": 83,
                            "wordSum": 16,
                            "tokenSum": 20,
                            "tokenCount": 20
                        }
                    ],
                    "wordSum": 19,
                    "tokenSum": 30,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[28th February 2013]",
                    "tokenCum": 94,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 151,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Power to destroy or otherwise dispose of property",
                            "tokenCum": 159,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "After section 42 of the Prison Act 1952 insert\u2014",
                                    "tokenCum": 176,
                                    "wordSum": 9,
                                    "tokenSum": 19,
                                    "tokenCount": 19
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "###",
                                    "text": "\u201cDisposal of property",
                                    "tokenCum": 182,
                                    "children": [
                                        {
                                            "type": "Heading",
                                            "prefix": "#### 42",
                                            "text": "ADisposal of unauthorised or unattributable property",
                                            "tokenCum": 197,
                                            "children": [
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(1)",
                                                    "text": "The governor or director of a prison may destroy or otherwise dispose of,or arrange for the destruction or other disposal of\u2014",
                                                    "tokenCum": 223,
                                                    "children": [
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "an article found in the possession of a prisoner who is not authorised tohave it in his or her possession, or",
                                                            "tokenCum": 247,
                                                            "wordSum": 21,
                                                            "tokenSum": 29,
                                                            "tokenCount": 29
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "an article found inside the prison or in a prisoner escort vehicle,otherwise than in the possession of a prisoner, where\u2014",
                                                            "tokenCum": 273,
                                                            "wordSum": 20,
                                                            "tokenSum": 31,
                                                            "tokenCount": 31
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(i)",
                                                            "text": "the owner of the article is a prisoner who is not authorised to have it inhis or her possession, or",
                                                            "tokenCum": 296,
                                                            "wordSum": 20,
                                                            "tokenSum": 28,
                                                            "tokenCount": 28
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(ii)",
                                                            "text": "the owner of the article cannot be ascertained.",
                                                            "tokenCum": 307,
                                                            "wordSum": 8,
                                                            "tokenSum": 16,
                                                            "tokenCount": 16
                                                        }
                                                    ],
                                                    "wordSum": 90,
                                                    "tokenSum": 134,
                                                    "tokenCount": 29
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(2)",
                                                    "text": "An article which a prisoner is authorised to have in his or her possessionis to be treated for the purposes of subsection (1) as not so authorised wherethe governor or director of the prison reasonably believes that the article isbeing, has been or may be used for any of the purposes mentioned in subsection(3).",
                                                    "tokenCum": 378,
                                                    "wordSum": 54,
                                                    "tokenSum": 75,
                                                    "tokenCount": 75
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(3)",
                                                    "text": "Those purposes are\u2014",
                                                    "tokenCum": 382,
                                                    "children": [
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "concealing an article which a prisoner is not authorised to have in his orher possession;",
                                                            "tokenCum": 401,
                                                            "wordSum": 15,
                                                            "tokenSum": 24,
                                                            "tokenCount": 24
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "causing harm to the prisoner or others;",
                                                            "tokenCum": 409,
                                                            "wordSum": 7,
                                                            "tokenSum": 13,
                                                            "tokenCount": 13
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(c)",
                                                            "text": "prejudicing the security or operation of the prison.",
                                                            "tokenCum": 420,
                                                            "wordSum": 8,
                                                            "tokenSum": 16,
                                                            "tokenCount": 16
                                                        }
                                                    ],
                                                    "wordSum": 33,
                                                    "tokenSum": 61,
                                                    "tokenCount": 7
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(4)",
                                                    "text": "Where a prisoner is authorised to have an article in his or her possessionin a particular part of the prison, subsection (1)(a) or (b)(i) applies onlywhere the property is found otherwise than in that part.",
                                                    "tokenCum": 469,
                                                    "wordSum": 35,
                                                    "tokenSum": 53,
                                                    "tokenCount": 53
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(5)",
                                                    "text": "In this section\u2014",
                                                    "tokenCum": 473,
                                                    "children": [
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "\u201cauthorised\u201d means authorised in accordance with prison rules or by thegovernor or director of the prison;",
                                                            "tokenCum": 498,
                                                            "wordSum": 16,
                                                            "tokenSum": 30,
                                                            "tokenCount": 30
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "\u201cprisoner escort vehicle\u201d means a vehicle used for taking a prisoner to orfrom a prison or other place while in custody;",
                                                            "tokenCum": 527,
                                                            "wordSum": 21,
                                                            "tokenSum": 34,
                                                            "tokenCount": 34
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(c)",
                                                            "text": "references to disposing of an article include selling it.",
                                                            "tokenCum": 539,
                                                            "wordSum": 9,
                                                            "tokenSum": 17,
                                                            "tokenCount": 17
                                                        }
                                                    ],
                                                    "wordSum": 49,
                                                    "tokenSum": 89,
                                                    "tokenCount": 7
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(6)",
                                                    "text": "The power under subsection (1)\u2014",
                                                    "tokenCum": 548,
                                                    "children": [
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "may be exercised in relation to a relevant article found before the day onwhich this section comes into force if the article remains unclaimed at theend of six months beginning with that day;",
                                                            "tokenCum": 587,
                                                            "wordSum": 33,
                                                            "tokenSum": 44,
                                                            "tokenCount": 44
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "may not otherwise be exercised in relation to an article found before thatday.",
                                                            "tokenCum": 603,
                                                            "wordSum": 13,
                                                            "tokenSum": 21,
                                                            "tokenCount": 21
                                                        }
                                                    ],
                                                    "wordSum": 51,
                                                    "tokenSum": 78,
                                                    "tokenCount": 12
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(7)",
                                                    "text": "In subsection (6)(a) \u201crelevant article\u201d means an article specified insection 40A(3)(c) or (d) (cameras and sound-recording devices) or section40D(3B) (devices capable of transmitting or receiving images, sounds orinformation by electronic communications, etc).\u201d",
                                                    "tokenCum": 677,
                                                    "wordSum": 32,
                                                    "tokenSum": 78,
                                                    "tokenCount": 78
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(2)",
                                                    "text": "In section 43 of that Act (power of Secretary of State to provide youngoffender institutions and secure training centres), in subsections (4) and(4A), for \u201cand 36\u201d substitute \u201c, 36 and 42A\u201d.",
                                                    "tokenCum": 733,
                                                    "wordSum": 31,
                                                    "tokenSum": 60,
                                                    "tokenCount": 60
                                                }
                                            ],
                                            "wordSum": 381,
                                            "tokenSum": 646,
                                            "tokenCount": 17
                                        }
                                    ],
                                    "wordSum": 384,
                                    "tokenSum": 654,
                                    "tokenCount": 7
                                }
                            ],
                            "wordSum": 401,
                            "tokenSum": 682,
                            "tokenCount": 8
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Extent, commencement and short title",
                            "tokenCum": 742,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act extends to England and Wales only.",
                                    "tokenCum": 751,
                                    "wordSum": 8,
                                    "tokenSum": 11,
                                    "tokenCount": 11
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Section 1 comes into force on such day as the Secretary of State may byorder made by statutory instrument appoint.",
                                    "tokenCum": 776,
                                    "wordSum": 20,
                                    "tokenSum": 27,
                                    "tokenCount": 27
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This section comes into force on the day on which this Act is passed.",
                                    "tokenCum": 791,
                                    "wordSum": 14,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "This Act may be cited as the Prisons (Property) Act 2013.",
                                    "tokenCum": 811,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 811,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 11,
                                    "tokenSum": 25,
                                    "tokenCount": 21
                                }
                            ],
                            "wordSum": 58,
                            "tokenSum": 90,
                            "tokenCount": 9
                        }
                    ],
                    "wordSum": 495,
                    "tokenSum": 830,
                    "tokenCount": 57
                }
            ],
