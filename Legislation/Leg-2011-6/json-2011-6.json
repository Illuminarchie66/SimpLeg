    "children": [
        {
            "type": "Text",
            "prefix": "",
            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2011/6/introduction/enacted?view=plain)",
            "tokenCum": 43,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Sports Grounds Safety Authority Act 2011",
                    "tokenCum": 56,
                    "wordSum": 6,
                    "tokenSum": 14,
                    "tokenCount": 13
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2011 CHAPTER 6",
                    "tokenCum": 64,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to confer further powers on the Football Licensing Authority and toamend its name; and for connected purposes.",
                            "tokenCum": 90,
                            "wordSum": 19,
                            "tokenSum": 27,
                            "tokenCount": 27
                        }
                    ],
                    "wordSum": 22,
                    "tokenSum": 36,
                    "tokenCount": 8
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[12th July 2011]",
                    "tokenCum": 101,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 158,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "##",
                            "text": "Part 1Sports Grounds Safety Authority",
                            "tokenCum": 169,
                            "children": [
                                {
                                    "type": "Heading",
                                    "prefix": "### 1",
                                    "text": "Football Licensing Authority to become Sports Grounds Safety Authority",
                                    "tokenCum": 184,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The body established by section 8 of, and Schedule 2 to, the FootballSpectators Act 1989\u2014",
                                            "tokenCum": 212,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "is to continue in being, but",
                                                    "tokenCum": 219,
                                                    "wordSum": 6,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "instead of being called the Football Licensing Authority is to be calledthe Sports Grounds Safety Authority.",
                                                    "tokenCum": 243,
                                                    "wordSum": 16,
                                                    "tokenSum": 28,
                                                    "tokenCount": 28
                                                }
                                            ],
                                            "wordSum": 37,
                                            "tokenSum": 70,
                                            "tokenCount": 30
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "Schedule 1 contains further provisions relating to the Authority.",
                                            "tokenCum": 258,
                                            "wordSum": 9,
                                            "tokenSum": 18,
                                            "tokenCount": 18
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "The expenses of the Authority are to be paid by the Secretary of State.",
                                            "tokenCum": 275,
                                            "wordSum": 14,
                                            "tokenSum": 20,
                                            "tokenCount": 20
                                        }
                                    ],
                                    "wordSum": 69,
                                    "tokenSum": 125,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 2",
                                    "text": "Advice to Ministers of the Crown",
                                    "tokenCum": 283,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The Authority\u2014",
                                            "tokenCum": 287,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "may provide relevant advice to a Minister of the Crown, and",
                                                    "tokenCum": 299,
                                                    "wordSum": 11,
                                                    "tokenSum": 16,
                                                    "tokenCount": 16
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "if requested to do so by a Minister of the Crown, must provide relevantadvice to that Minister.",
                                                    "tokenCum": 320,
                                                    "wordSum": 17,
                                                    "tokenSum": 25,
                                                    "tokenCount": 25
                                                }
                                            ],
                                            "wordSum": 30,
                                            "tokenSum": 48,
                                            "tokenCount": 6
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "\u201cRelevant advice\u201d means\u2014",
                                            "tokenCum": 327,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "advice in relation to safety at sports grounds generally, or",
                                                    "tokenCum": 338,
                                                    "wordSum": 10,
                                                    "tokenSum": 15,
                                                    "tokenCount": 15
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "advice with respect to the exercise of any of the functions of the Ministerin question under the enactments specified in subsection (3).",
                                                    "tokenCum": 366,
                                                    "wordSum": 22,
                                                    "tokenSum": 32,
                                                    "tokenCount": 32
                                                }
                                            ],
                                            "wordSum": 35,
                                            "tokenSum": 57,
                                            "tokenCount": 9
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "The enactments are\u2014",
                                            "tokenCum": 372,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "the Safety of Sports Grounds Act 1975,",
                                                    "tokenCum": 386,
                                                    "wordSum": 7,
                                                    "tokenSum": 18,
                                                    "tokenCount": 18
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "Part 3 of the Fire Safety and Safety of Places of Sport Act 1987 (safety ofstands at sports grounds), and",
                                                    "tokenCum": 419,
                                                    "wordSum": 20,
                                                    "tokenSum": 37,
                                                    "tokenCount": 37
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "the Football Spectators Act 1989.",
                                                    "tokenCum": 430,
                                                    "wordSum": 5,
                                                    "tokenSum": 15,
                                                    "tokenCount": 15
                                                }
                                            ],
                                            "wordSum": 35,
                                            "tokenSum": 79,
                                            "tokenCount": 8
                                        }
                                    ],
                                    "wordSum": 106,
                                    "tokenSum": 194,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 3",
                                    "text": "Advice to bodies or persons in England and Wales",
                                    "tokenCum": 440,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The Authority may provide advice relating to safety at sports grounds inEngland or Wales to\u2014",
                                            "tokenCum": 460,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "local authorities, or",
                                                    "tokenCum": 464,
                                                    "wordSum": 3,
                                                    "tokenSum": 8,
                                                    "tokenCount": 8
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "subject to subsection (2), other bodies or persons.",
                                                    "tokenCum": 476,
                                                    "wordSum": 8,
                                                    "tokenSum": 16,
                                                    "tokenCount": 16
                                                }
                                            ],
                                            "wordSum": 26,
                                            "tokenSum": 47,
                                            "tokenCount": 22
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "The Authority may not under subsection (1) provide advice to\u2014",
                                            "tokenCum": 491,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "a Minister of the Crown, or",
                                                    "tokenCum": 498,
                                                    "wordSum": 6,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "a body or person specified in section 4(1)(a), (b) or (c).",
                                                    "tokenCum": 519,
                                                    "wordSum": 11,
                                                    "tokenSum": 25,
                                                    "tokenCount": 25
                                                }
                                            ],
                                            "wordSum": 27,
                                            "tokenSum": 54,
                                            "tokenCount": 17
                                        }
                                    ],
                                    "wordSum": 62,
                                    "tokenSum": 113,
                                    "tokenCount": 11
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 4",
                                    "text": "Advice to bodies or persons outside England and Wales",
                                    "tokenCum": 529,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The Authority may, if the conditions in subsection (2) are met, provideadvice relating to the safety of sports grounds to\u2014",
                                            "tokenCum": 559,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "the government of a territory outside the United Kingdom,",
                                                    "tokenCum": 569,
                                                    "wordSum": 9,
                                                    "tokenSum": 14,
                                                    "tokenCount": 14
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "an international organisation, or",
                                                    "tokenCum": 574,
                                                    "wordSum": 4,
                                                    "tokenSum": 9,
                                                    "tokenCount": 9
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "a body or person not falling within paragraph (a) or (b) whose functions,activities or responsibilities relate in whole or in part to the safety ofsports grounds outside England and Wales.",
                                                    "tokenCum": 616,
                                                    "wordSum": 30,
                                                    "tokenSum": 46,
                                                    "tokenCount": 46
                                                }
                                            ],
                                            "wordSum": 63,
                                            "tokenSum": 102,
                                            "tokenCount": 32
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "The conditions are that\u2014",
                                            "tokenCum": 621,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "the provision of the advice is at the request of the body or personconcerned, and",
                                                    "tokenCum": 641,
                                                    "wordSum": 15,
                                                    "tokenSum": 24,
                                                    "tokenCount": 24
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "the Secretary of State consents to the provision of the advice.",
                                                    "tokenCum": 654,
                                                    "wordSum": 11,
                                                    "tokenSum": 17,
                                                    "tokenCount": 17
                                                }
                                            ],
                                            "wordSum": 30,
                                            "tokenSum": 49,
                                            "tokenCount": 7
                                        }
                                    ],
                                    "wordSum": 102,
                                    "tokenSum": 163,
                                    "tokenCount": 11
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 5",
                                    "text": "Supplementary ",
                                    "tokenCum": 658,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The Authority may, with the consent of the Secretary of State, charge a feefor the provision of advice under\u2014",
                                            "tokenCum": 683,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "section 4 (advice to bodies or persons outside England and Wales), or",
                                                    "tokenCum": 699,
                                                    "wordSum": 12,
                                                    "tokenSum": 20,
                                                    "tokenCount": 20
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "if the advice is provided at the request of the recipient, section 3(advice to bodies or persons in England and Wales).",
                                                    "tokenCum": 727,
                                                    "wordSum": 21,
                                                    "tokenSum": 32,
                                                    "tokenCount": 32
                                                }
                                            ],
                                            "wordSum": 52,
                                            "tokenSum": 80,
                                            "tokenCount": 27
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "A fee charged under subsection (1) must not exceed the cost of providingthe advice.",
                                            "tokenCum": 746,
                                            "wordSum": 14,
                                            "tokenSum": 22,
                                            "tokenCount": 22
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "Fees received by the Authority by virtue of subsection (1) are to betreated for the purposes of section 1(3) as reducing the expenses of theAuthority.",
                                            "tokenCum": 785,
                                            "wordSum": 25,
                                            "tokenSum": 42,
                                            "tokenCount": 42
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(4)",
                                            "text": "A consent under section 4(2)(b) or subsection (1) may be given\u2014",
                                            "tokenCum": 806,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "generally, ",
                                                    "tokenCum": 809,
                                                    "wordSum": 1,
                                                    "tokenSum": 7,
                                                    "tokenCount": 7
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "in relation to any particular advice, or",
                                                    "tokenCum": 817,
                                                    "wordSum": 7,
                                                    "tokenSum": 12,
                                                    "tokenCount": 12
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "in relation to advice of a particular description.",
                                                    "tokenCum": 826,
                                                    "wordSum": 8,
                                                    "tokenSum": 13,
                                                    "tokenCount": 13
                                                }
                                            ],
                                            "wordSum": 27,
                                            "tokenSum": 56,
                                            "tokenCount": 23
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(5)",
                                            "text": "In this Part\u2014",
                                            "tokenCum": 830,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "\u201clocal authority\u201d and \u201csports ground\u201d have the same meaning as in theSafety of Sports Grounds Act 1975 (see section 17(1) of that Act), and",
                                                    "tokenCum": 874,
                                                    "wordSum": 24,
                                                    "tokenSum": 48,
                                                    "tokenCount": 48
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "\u201cMinister of the Crown\u201d has the same meaning as in the Ministers of theCrown Act 1975.",
                                                    "tokenCum": 901,
                                                    "wordSum": 16,
                                                    "tokenSum": 31,
                                                    "tokenCount": 31
                                                }
                                            ],
                                            "wordSum": 43,
                                            "tokenSum": 86,
                                            "tokenCount": 6
                                        }
                                    ],
                                    "wordSum": 162,
                                    "tokenSum": 292,
                                    "tokenCount": 5
                                }
                            ],
                            "wordSum": 506,
                            "tokenSum": 899,
                            "tokenCount": 11
                        },
                        {
                            "type": "Heading",
                            "prefix": "##",
                            "text": "Part 2Supplementary",
                            "tokenCum": 907,
                            "children": [
                                {
                                    "type": "Heading",
                                    "prefix": "### 6",
                                    "text": "Consequential amendments etc.",
                                    "tokenCum": 915,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "Schedule 2 contains consequential amendments.",
                                            "tokenCum": 926,
                                            "wordSum": 5,
                                            "tokenSum": 14,
                                            "tokenCount": 14
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "Schedule 3 contains repeals and revocations.",
                                            "tokenCum": 938,
                                            "wordSum": 6,
                                            "tokenSum": 15,
                                            "tokenCount": 15
                                        }
                                    ],
                                    "wordSum": 14,
                                    "tokenSum": 39,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 7",
                                    "text": "Extent ",
                                    "tokenCum": 941,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "This Act extends to England and Wales only (subject to subsection (2)).",
                                            "tokenCum": 957,
                                            "wordSum": 12,
                                            "tokenSum": 19,
                                            "tokenCount": 19
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "An amendment or repeal of a provision by Schedule 2 (consequentialamendments) or 3 (repeals and revocations) has the same extent as thatprovision.",
                                            "tokenCum": 1001,
                                            "wordSum": 22,
                                            "tokenSum": 47,
                                            "tokenCount": 47
                                        }
                                    ],
                                    "wordSum": 35,
                                    "tokenSum": 71,
                                    "tokenCount": 4
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 8",
                                    "text": "Commencement ",
                                    "tokenCum": 1005,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "Part 1 and the Schedules come into force on such day as the Secretary ofState may by order made by statutory instrument appoint.",
                                            "tokenCum": 1035,
                                            "wordSum": 23,
                                            "tokenSum": 33,
                                            "tokenCount": 33
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "Such an order may\u2014",
                                            "tokenCum": 1040,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "appoint different days for different purposes;",
                                                    "tokenCum": 1047,
                                                    "wordSum": 6,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "include transitional or saving provisions.",
                                                    "tokenCum": 1055,
                                                    "wordSum": 5,
                                                    "tokenSum": 12,
                                                    "tokenCount": 12
                                                }
                                            ],
                                            "wordSum": 15,
                                            "tokenSum": 31,
                                            "tokenCount": 7
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "This Part comes into force on the day on which this Act is passed.",
                                            "tokenCum": 1070,
                                            "wordSum": 14,
                                            "tokenSum": 18,
                                            "tokenCount": 18
                                        }
                                    ],
                                    "wordSum": 53,
                                    "tokenSum": 88,
                                    "tokenCount": 5
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "### 9",
                                    "text": "Short title",
                                    "tokenCum": 1072,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "This Act may be cited as the Sports Grounds Safety Authority Act 2011.",
                                            "tokenCum": 1094,
                                            "wordSum": 13,
                                            "tokenSum": 25,
                                            "tokenCount": 25
                                        }
                                    ],
                                    "wordSum": 15,
                                    "tokenSum": 29,
                                    "tokenCount": 3
                                }
                            ],
                            "wordSum": 119,
                            "tokenSum": 234,
                            "tokenCount": 6
                        }
                    ],
                    "wordSum": 661,
                    "tokenSum": 1191,
                    "tokenCount": 57
                }
            ],
            "wordSum": 693,
            "tokenSum": 1297,
            "tokenCount": 43
        }
    ],
