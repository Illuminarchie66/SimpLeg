                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2007 CHAPTER 2",
                    "tokenCum": 69,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to permit expenditure in preparation for the imposition of a tax on theincrease in the value of land resulting from the grant of permission fordevelopment.",
                            "tokenCum": 105,
                            "wordSum": 27,
                            "tokenSum": 37,
                            "tokenCount": 37
                        }
                    ],
                    "wordSum": 30,
                    "tokenSum": 46,
                    "tokenCount": 8
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[20th March 2007]",
                    "tokenCum": 116,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 173,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Preparatory expenditure",
                            "tokenCum": 179,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The persons listed in subsection (2) may incur expenditure for the purposeof or in connection with preparing for the imposition of a tax on the increasein the value of land resulting from the grant of permission for development.",
                                    "tokenCum": 228,
                                    "wordSum": 38,
                                    "tokenSum": 51,
                                    "tokenCount": 51
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Those persons are\u2014",
                                    "tokenCum": 232,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the Commissioners for Her Majesty\u2019s Revenue and Customs,",
                                            "tokenCum": 246,
                                            "wordSum": 8,
                                            "tokenSum": 17,
                                            "tokenCount": 17
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "the Secretary of State, and",
                                            "tokenCum": 252,
                                            "wordSum": 5,
                                            "tokenSum": 9,
                                            "tokenCount": 9
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "a Northern Ireland Department.",
                                            "tokenCum": 257,
                                            "wordSum": 4,
                                            "tokenSum": 8,
                                            "tokenCount": 8
                                        }
                                    ],
                                    "wordSum": 20,
                                    "tokenSum": 40,
                                    "tokenCount": 5
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "Expenditure by virtue of subsection (2)(a) or (b) shall be paid out ofmoney provided by Parliament.",
                                    "tokenCum": 285,
                                    "wordSum": 16,
                                    "tokenSum": 30,
                                    "tokenCount": 30
                                }
                            ],
                            "wordSum": 76,
                            "tokenSum": 128,
                            "tokenCount": 6
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Extent ",
                            "tokenCum": 288,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "This Act extends to the United Kingdom.",
                                    "tokenCum": 296,
                                    "wordSum": 7,
                                    "tokenSum": 10,
                                    "tokenCount": 10
                                }
                            ],
                            "wordSum": 8,
                            "tokenSum": 14,
                            "tokenCount": 3
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Short Title",
                            "tokenCum": 298,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "This Act may be cited as the Planning-gain Supplement (Preparations) Act 2007.",
