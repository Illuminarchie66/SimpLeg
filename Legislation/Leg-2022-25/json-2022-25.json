            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2022/25/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Motor Vehicles (Compulsory Insurance) Act 2022",
                    "tokenCum": 62,
                    "wordSum": 6,
                    "tokenSum": 19,
                    "tokenCount": 18
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2022 CHAPTER 25",
                    "tokenCum": 71,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to amend retained EU law relating to compulsory insurance for the useof motor vehicles; and for connected purposes.",
                            "tokenCum": 99,
                            "wordSum": 20,
                            "tokenSum": 29,
                            "tokenCount": 29
                        }
                    ],
                    "wordSum": 23,
                    "tokenSum": 39,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[28th April 2022]",
                    "tokenCum": 110,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 167,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Retained EU law relating to compulsory insurance for motor vehicles",
                            "tokenCum": 182,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "After section 156 of the Road Traffic Act 1988 insert\u2014",
                                    "tokenCum": 201,
                                    "wordSum": 10,
                                    "tokenSum": 21,
                                    "tokenCount": 21
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "###",
                                    "text": "\u201cRetained EU law relating to compulsory insurance",
                                    "tokenCum": 214,
                                    "children": [
                                        {
                                            "type": "Heading",
                                            "prefix": "#### 156",
                                            "text": "ARetained EU law relating to compulsory insurance",
                                            "tokenCum": 227,
                                            "children": [
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(1)",
                                                    "text": "To the extent that Article 3 of the 2009 Motor Insurance Directive (as ithad effect at any time) is relevant to any question as to the interpretationor effect of any provision of this Part, references in that Article toliability in respect of the use of vehicles are to be read as not includingliability in respect of the use in Great Britain of vehicles\u2014",
                                                    "tokenCum": 308,
                                                    "children": [
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "other than motor vehicles, or",
                                                            "tokenCum": 314,
                                                            "wordSum": 5,
                                                            "tokenSum": 11,
                                                            "tokenCount": 11
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "otherwise than on a road or other public place.",
                                                            "tokenCum": 324,
                                                            "wordSum": 9,
                                                            "tokenSum": 15,
                                                            "tokenCount": 15
                                                        }
                                                    ],
                                                    "wordSum": 77,
                                                    "tokenSum": 111,
                                                    "tokenCount": 84
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(2)",
                                                    "text": "Subsection (1) does not apply in relation to any question for the purposesof section 145(3)(aa) or (b) as to the interpretation or effect of the law oncompulsory insurance of, or applicable in, a member State or Northern Ireland.",
                                                    "tokenCum": 383,
                                                    "wordSum": 38,
                                                    "tokenSum": 63,
                                                    "tokenCount": 63
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(3)",
                                                    "text": "Relevant section 4 rights cease to be recognised and available so far asthey relate to compensation in connection with the use in Great Britain ofvehicles\u2014",
                                                    "tokenCum": 419,
                                                    "children": [
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "other than motor vehicles, or",
                                                            "tokenCum": 425,
                                                            "wordSum": 5,
                                                            "tokenSum": 11,
                                                            "tokenCount": 11
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "otherwise than on a road or other public place.",
                                                            "tokenCum": 435,
                                                            "wordSum": 9,
                                                            "tokenSum": 15,
                                                            "tokenCount": 15
                                                        }
                                                    ],
                                                    "wordSum": 39,
                                                    "tokenSum": 66,
                                                    "tokenCount": 39
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(4)",
                                                    "text": "Accordingly, to the extent that it is inconsistent with subsection (1) or(3), retained case law ceases to have effect.",
                                                    "tokenCum": 465,
                                                    "wordSum": 19,
                                                    "tokenSum": 34,
                                                    "tokenCount": 34
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(5)",
                                                    "text": "In this section\u2014",
                                                    "tokenCum": 469,
                                                    "children": [
                                                        {
                                                            "type": "Bullet point",
                                                            "prefix": "*",
                                                            "text": "  * \u201cthe 2009 Motor Insurance Directive\u201d means [Directive 2009/103/EC](/european/directive/2009/0103?view=plain \"Go to item of legislation\") of the European Parliament and of the Council of 16 September 2009 relating to insurance against civil liability in respect of the use of motor vehicles, and the enforcement of the obligation to insure against such liability;",
                                                            "tokenCum": 581,
                                                            "wordSum": 51,
                                                            "tokenSum": 117,
                                                            "tokenCount": 117
                                                        },
                                                        {
                                                            "type": "Bullet point",
                                                            "prefix": "*",
                                                            "text": "  * \u201crelevant section 4 rights\u201d means section 4 rights which\u2014",
                                                            "tokenCum": 598,
                                                            "wordSum": 10,
                                                            "tokenSum": 22,
                                                            "tokenCount": 22
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "are recognised and available in the law of England and Wales or the law ofScotland, and",
                                                            "tokenCum": 598,
                                                            "wordSum": 16,
                                                            "tokenSum": 26,
                                                            "tokenCount": 26
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "derive from the obligation imposed on the United Kingdom by Article 10 of the2009 Motor Insurance Directive as it had effect immediately before IPcompletion day (which relates to compensation in connection with the use ofvehicles in cases where drivers are uninsured or untraced);",
                                                            "tokenCum": 619,
                                                            "children": [
                                                                {
                                                                    "type": "Bullet point",
                                                                    "prefix": "*",
                                                                    "text": "  * \u201cretained case law\u201d has the same meaning as in the European Union (Withdrawal) Act 2018 (see section 6(7) of that Act);",
                                                                    "tokenCum": 727,
                                                                    "wordSum": 22,
                                                                    "tokenSum": 46,
                                                                    "tokenCount": 46
                                                                },
                                                                {
                                                                    "type": "Bullet point",
                                                                    "prefix": "*",
                                                                    "text": "  * \u201csection 4 rights\u201d means rights, powers, liabilities, obligations, restrictions, remedies and procedures which continue to be recognised and available in domestic law by virtue of section 4 of the European Union (Withdrawal) Act 2018 (saving for rights etc under section 2(1) of the ECA), including those rights, powers, liabilities, obligations, restrictions, remedies and procedures\u2014",
                                                                    "tokenCum": 823,
                                                                    "wordSum": 55,
                                                                    "tokenSum": 102,
                                                                    "tokenCount": 102
                                                                }
                                                            ],
                                                            "wordSum": 120,
                                                            "tokenSum": 221,
                                                            "tokenCount": 72
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(a)",
                                                            "text": "as modified by domestic law from time to time, and",
                                                            "tokenCum": 823,
                                                            "wordSum": 10,
                                                            "tokenSum": 16,
                                                            "tokenCount": 16
                                                        },
                                                        {
                                                            "type": "Alpha point",
                                                            "prefix": "(b)",
                                                            "text": "as they apply to the Crown.",
                                                            "tokenCum": 834,
                                                            "wordSum": 6,
                                                            "tokenSum": 12,
                                                            "tokenCount": 12
                                                        }
                                                    ],
                                                    "wordSum": 216,
                                                    "tokenSum": 422,
                                                    "tokenCount": 7
                                                },
                                                {
                                                    "type": "Digit point",
                                                    "prefix": "(6)",
                                                    "text": "Nothing in this section applies in relation to the use of a vehicle beforethe day on which section 1 of the Motor Vehicles (Compulsory Insurance) Act2022 comes into force.\u201d",
                                                    "tokenCum": 886,
                                                    "wordSum": 29,
                                                    "tokenSum": 49,
                                                    "tokenCount": 49
                                                }
                                            ],
                                            "wordSum": 425,
                                            "tokenSum": 761,
                                            "tokenCount": 15
                                        }
                                    ],
                                    "wordSum": 432,
                                    "tokenSum": 776,
                                    "tokenCount": 14
                                }
                            ],
                            "wordSum": 452,
                            "tokenSum": 813,
                            "tokenCount": 15
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Commencement, extent and short title",
                            "tokenCum": 894,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act comes into force at the end of the period of two months beginningwith the day on which it is passed.",
                                    "tokenCum": 918,
                                    "wordSum": 22,
                                    "tokenSum": 26,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act extends to England and Wales and Scotland.",
                                    "tokenCum": 928,
                                    "wordSum": 9,
                                    "tokenSum": 12,
                                    "tokenCount": 12
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This Act may be cited as the Motor Vehicles (Compulsory Insurance) Act2022.",
                                    "tokenCum": 954,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 954,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 12,
                                    "tokenSum": 31,
                                    "tokenCount": 27
                                }
                            ],
                            "wordSum": 48,
                            "tokenSum": 78,
                            "tokenCount": 8
                        }
                    ],
                    "wordSum": 536,
                    "tokenSum": 949,
                    "tokenCount": 57
                }
            ],
