            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2015/34/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Northern Ireland (Welfare Reform) Act 2015",
                    "tokenCum": 58,
                    "wordSum": 6,
                    "tokenSum": 15,
                    "tokenCount": 14
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2015 CHAPTER 34",
                    "tokenCum": 67,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision in connection with social security and child supportmaintenance in Northern Ireland; to make provision in connection witharrangements under section 1 of the Employment and Training Act (NorthernIreland) 1950; and for connected purposes.",
                            "tokenCum": 125,
                            "wordSum": 37,
                            "tokenSum": 59,
                            "tokenCount": 59
                        }
                    ],
                    "wordSum": 40,
                    "tokenSum": 69,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[25th November 2015]",
                    "tokenCum": 136,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 193,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Power to make provision in connection with social security, child supportmaintenance and arrangements for employment in Northern Ireland",
                            "tokenCum": 217,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "Her Majesty may by Order in Council make provision in connection with\u2014",
                                    "tokenCum": 230,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "social security and child support maintenance in Northern Ireland, and",
                                            "tokenCum": 241,
                                            "wordSum": 10,
                                            "tokenSum": 14,
                                            "tokenCount": 14
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "arrangements under section 1 of the Employment and Training Act (NorthernIreland) 1950 (employment and training for employment).",
                                            "tokenCum": 275,
                                            "wordSum": 17,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        }
                                    ],
                                    "wordSum": 39,
                                    "tokenSum": 66,
                                    "tokenCount": 14
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "An Order in Council under subsection (1) may, in particular, confer poweron the Secretary of State or a Northern Ireland department to make provisionby order or regulations.",
                                    "tokenCum": 311,
                                    "wordSum": 27,
                                    "tokenSum": 38,
                                    "tokenCount": 38
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "An Order in Council under subsection (1) or an order or regulations undersubsection (2) may in particular\u2014",
                                    "tokenCum": 337,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "amend, repeal, revoke or otherwise modify any enactment,",
                                            "tokenCum": 353,
                                            "wordSum": 8,
                                            "tokenSum": 19,
                                            "tokenCount": 19
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "apply any enactment with or without modification,",
                                            "tokenCum": 363,
                                            "wordSum": 7,
                                            "tokenSum": 13,
                                            "tokenCount": 13
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "provide for a person to exercise a discretion in dealing with any matter,",
                                            "tokenCum": 379,
                                            "wordSum": 13,
                                            "tokenSum": 19,
                                            "tokenCount": 19
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(d)",
                                            "text": "create or amend a criminal offence or impose or amend a penalty,",
                                            "tokenCum": 396,
                                            "wordSum": 12,
                                            "tokenSum": 20,
                                            "tokenCount": 20
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(e)",
                                            "text": "make different provision for different purposes or areas, and",
                                            "tokenCum": 406,
                                            "wordSum": 9,
                                            "tokenSum": 13,
                                            "tokenCount": 13
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(f)",
                                            "text": "make incidental, supplementary, consequential, transitory or transitionalprovision or savings.",
                                            "tokenCum": 428,
                                            "wordSum": 9,
                                            "tokenSum": 25,
                                            "tokenCount": 25
                                        }
                                    ],
                                    "wordSum": 75,
                                    "tokenSum": 137,
                                    "tokenCount": 27
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "An Order in Council under subsection (1) that confers power to make anorder or regulations may provide that\u2014",
                                    "tokenCum": 453,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the power is exercisable either by statutory instrument or by statutoryrule where the power is conferred on the Secretary of State, or",
                                            "tokenCum": 484,
                                            "wordSum": 22,
                                            "tokenSum": 34,
                                            "tokenCount": 34
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "the power is exercisable by statutory rule where the power is conferred ona Northern Ireland department.",
                                            "tokenCum": 507,
                                            "wordSum": 16,
                                            "tokenSum": 26,
                                            "tokenCount": 26
                                        }
                                    ],
                                    "wordSum": 56,
                                    "tokenSum": 87,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "No recommendation is to be made to Her Majesty to make an Order in Councilunder subsection (1) unless a draft of the statutory instrument containing theOrder has been laid before, and approved by a resolution of, each House ofParliament.",
                                    "tokenCum": 558,
                                    "wordSum": 39,
                                    "tokenSum": 53,
                                    "tokenCount": 53
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "In this section\u2014",
                                    "tokenCum": 562,
                                    "children": [
                                        {
                                            "type": "Bullet point",
                                            "prefix": "*",
                                            "text": "  * \u201cenactment\u201d means an enactment contained in, or in an instrument made under\u2014 ",
                                            "tokenCum": 585,
                                            "wordSum": 13,
                                            "tokenSum": 26,
                                            "tokenCount": 26
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "an Act of Parliament, or",
                                            "tokenCum": 585,
                                            "wordSum": 5,
                                            "tokenSum": 9,
                                            "tokenCount": 9
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "Northern Ireland legislation;",
                                            "tokenCum": 591,
                                            "children": [
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * \u201cstatutory rule\u201d means statutory rule for the purposes of the Statutory Rules (Northern Ireland) Order 1979 ([S.I. 1979/1573 (N.I. 12)](/id/nisi/1979/1573?view=plain \"Go to item of legislation\")). ",
                                                    "tokenCum": 684,
                                                    "wordSum": 26,
                                                    "tokenSum": 92,
                                                    "tokenCount": 92
                                                }
                                            ],
                                            "wordSum": 29,
                                            "tokenSum": 100,
                                            "tokenCount": 7
                                        }
                                    ],
                                    "wordSum": 50,
                                    "tokenSum": 141,
                                    "tokenCount": 5
                                }
                            ],
                            "wordSum": 304,
                            "tokenSum": 547,
                            "tokenCount": 24
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Section 1: supplementary provision",
                            "tokenCum": 692,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "References to Acts of the Northern Ireland Assembly in any enactment orinstrument (whether passed or made before or after the coming into force ofthis Act) are to be read, so far as the context permits, as includingreferences to Orders in Council under section 1(1).",
                                    "tokenCum": 755,
                                    "wordSum": 44,
                                    "tokenSum": 65,
                                    "tokenCount": 65
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Subsection (1) does not apply to references to Acts of the Northern IrelandAssembly in section 6 of the Northern Ireland Act 1998.",
                                    "tokenCum": 788,
                                    "wordSum": 22,
                                    "tokenSum": 35,
                                    "tokenCount": 35
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "In subsection (1)\u2014",
                                    "tokenCum": 795,
                                    "children": [
                                        {
                                            "type": "Bullet point",
                                            "prefix": "*",
                                            "text": "  * \u201cenactment\u201d means an enactment contained in, or in an instrument made under\u2014 ",
                                            "tokenCum": 818,
                                            "wordSum": 13,
                                            "tokenSum": 26,
                                            "tokenCount": 26
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "an Act of Parliament,",
                                            "tokenCum": 818,
                                            "wordSum": 4,
                                            "tokenSum": 8,
                                            "tokenCount": 8
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "an Act of the Scottish Parliament,",
                                            "tokenCum": 823,
                                            "wordSum": 6,
                                            "tokenSum": 10,
                                            "tokenCount": 10
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "a Measure or Act of the National Assembly for Wales, or",
                                            "tokenCum": 830,
                                            "wordSum": 11,
                                            "tokenSum": 16,
                                            "tokenCount": 16
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(d)",
                                            "text": "Northern Ireland legislation, and",
                                            "tokenCum": 843,
                                            "children": [
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * \u201cinstrument\u201d includes a charter, contract or other document. ",
                                                    "tokenCum": 866,
                                                    "wordSum": 9,
                                                    "tokenSum": 21,
                                                    "tokenCount": 21
                                                }
                                            ],
                                            "wordSum": 13,
                                            "tokenSum": 30,
                                            "tokenCount": 8
                                        }
                                    ],
                                    "wordSum": 50,
                                    "tokenSum": 99,
                                    "tokenCount": 8
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "Orders in Council under section 1(1) may be omitted from any annual editionof statutory instruments required to be prepared under regulations made as aresult of section 8 of the Statutory Instruments Act 1946.",
                                    "tokenCum": 920,
                                    "wordSum": 33,
                                    "tokenSum": 56,
                                    "tokenCount": 56
                                }
                            ],
                            "wordSum": 153,
                            "tokenSum": 264,
                            "tokenCount": 8
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Extent, commencement, sunset and short title",
                            "tokenCum": 932,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act extends to England and Wales, Scotland and Northern Ireland.",
                                    "tokenCum": 945,
                                    "wordSum": 11,
                                    "tokenSum": 15,
                                    "tokenCount": 15
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act comes into force on the day on which it is passed.",
                                    "tokenCum": 959,
                                    "wordSum": 13,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "No Order in Council may be made under section 1(1) after 31 December 2016.",
                                    "tokenCum": 984,
                                    "wordSum": 14,
                                    "tokenSum": 27,
                                    "tokenCount": 27
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "This Act may be cited as the Northern Ireland (Welfare Reform) Act 2015.",
                                    "tokenCum": 1007,
