            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Holocaust (Return of Cultural Objects) Act 2009",
                    "tokenCum": 60,
                    "wordSum": 7,
                    "tokenSum": 17,
                    "tokenCount": 16
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2009 CHAPTER 16",
                    "tokenCum": 69,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to confer power to return certain cultural objects on grounds relatingto events occurring during the Nazi era.",
                            "tokenCum": 93,
                            "wordSum": 19,
                            "tokenSum": 25,
                            "tokenCount": 25
                        }
                    ],
                    "wordSum": 22,
                    "tokenSum": 35,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[12th November 2009]",
                    "tokenCum": 104,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 161,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Bodies to which this Act applies",
                            "tokenCum": 168,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "This Act applies to the following bodies\u2014",
                                    "tokenCum": 176,
                                    "wordSum": 7,
                                    "tokenSum": 10,
                                    "tokenCount": 10
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the Armouries, ",
                                    "tokenCum": 190,
                                    "wordSum": 8,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The British Library Board, ",
                                    "tokenCum": 198,
                                    "wordSum": 5,
                                    "tokenSum": 10,
                                    "tokenCount": 10
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Trustees of the British Museum, ",
                                    "tokenCum": 209,
                                    "wordSum": 7,
                                    "tokenSum": 13,
                                    "tokenCount": 13
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Trustees of the Imperial War Museum, ",
                                    "tokenCum": 221,
                                    "wordSum": 8,
                                    "tokenSum": 14,
                                    "tokenCount": 14
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees for the National Galleries of Scotland, ",
                                    "tokenCum": 237,
                                    "wordSum": 11,
                                    "tokenSum": 18,
                                    "tokenCount": 18
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the National Gallery, ",
                                    "tokenCum": 250,
                                    "wordSum": 9,
                                    "tokenSum": 15,
                                    "tokenCount": 15
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Trustees of the National Library of Scotland, ",
                                    "tokenCum": 263,
                                    "wordSum": 9,
                                    "tokenSum": 15,
                                    "tokenCount": 15
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Trustees of the National Maritime Museum, ",
                                    "tokenCum": 276,
                                    "wordSum": 8,
                                    "tokenSum": 15,
                                    "tokenCount": 15
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the National Museums and Galleries on Merseyside, ",
                                    "tokenCum": 298,
                                    "wordSum": 13,
                                    "tokenSum": 24,
                                    "tokenCount": 24
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the National Museums of Scotland, ",
                                    "tokenCum": 314,
                                    "wordSum": 11,
                                    "tokenSum": 18,
                                    "tokenCount": 18
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the National Portrait Gallery, ",
                                    "tokenCum": 329,
                                    "wordSum": 10,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Trustees of the Natural History Museum, ",
                                    "tokenCum": 341,
                                    "wordSum": 8,
                                    "tokenSum": 14,
                                    "tokenCount": 14
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the Royal Botanic Gardens, Kew, ",
                                    "tokenCum": 360,
                                    "wordSum": 11,
                                    "tokenSum": 21,
                                    "tokenCount": 21
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the Science Museum, ",
                                    "tokenCum": 373,
                                    "wordSum": 9,
                                    "tokenSum": 15,
                                    "tokenCount": 15
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the Tate Gallery, ",
                                    "tokenCum": 387,
                                    "wordSum": 9,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the Victoria and Albert Museum, ",
                                    "tokenCum": 402,
                                    "wordSum": 11,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Bullet point",
                                    "prefix": "*",
                                    "text": "  * The Board of Trustees of the Wallace Collection. ",
                                    "tokenCum": 416,
                                    "wordSum": 9,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                }
                            ],
                            "wordSum": 169,
                            "tokenSum": 292,
                            "tokenCount": 7
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Power to return victims\u2019 property",
                            "tokenCum": 423,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "A body to which this Act applies may transfer an object from itscollections if the following conditions are met.",
                                    "tokenCum": 444,
                                    "wordSum": 19,
                                    "tokenSum": 23,
                                    "tokenCount": 23
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Condition 1 is that the Advisory Panel has recommended the transfer.",
                                    "tokenCum": 461,
                                    "wordSum": 11,
                                    "tokenSum": 19,
                                    "tokenCount": 19
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "Condition 2 is that the Secretary of State has approved the AdvisoryPanel\u2019s recommendation.",
                                    "tokenCum": 482,
                                    "wordSum": 13,
                                    "tokenSum": 23,
                                    "tokenCount": 23
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "The Secretary of State may approve a recommendation for the transfer of anobject from the collections of a Scottish body only with the consent of theScottish Ministers.",
                                    "tokenCum": 516,
                                    "wordSum": 27,
                                    "tokenSum": 36,
                                    "tokenCount": 36
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "\u201cScottish body\u201d means\u2014",
                                    "tokenCum": 524,
                                    "children": [
                                        {
                                            "type": "Bullet point",
                                            "prefix": "*",
                                            "text": "  * The Board of Trustees for the National Galleries of Scotland, ",
                                            "tokenCum": 540,
                                            "wordSum": 11,
                                            "tokenSum": 19,
                                            "tokenCount": 19
                                        },
                                        {
                                            "type": "Bullet point",
                                            "prefix": "*",
                                            "text": "  * The Trustees of the National Library of Scotland, ",
                                            "tokenCum": 553,
                                            "wordSum": 9,
                                            "tokenSum": 16,
                                            "tokenCount": 16
                                        },
                                        {
                                            "type": "Bullet point",
                                            "prefix": "*",
                                            "text": "  * The Board of Trustees of the National Museums of Scotland. ",
                                            "tokenCum": 569,
                                            "wordSum": 11,
                                            "tokenSum": 19,
                                            "tokenCount": 19
                                        }
                                    ],
                                    "wordSum": 34,
                                    "tokenSum": 64,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "The power conferred by subsection (1) does not affect any trust orcondition subject to which any object is held.",
                                    "tokenCum": 594,
                                    "wordSum": 19,
                                    "tokenSum": 27,
                                    "tokenCount": 27
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(7)",
                                    "text": "The power conferred by subsection (1) is an additional power.",
                                    "tokenCum": 609,
                                    "wordSum": 10,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                }
                            ],
                            "wordSum": 138,
                            "tokenSum": 217,
                            "tokenCount": 7
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "\u201cAdvisory Panel\u201d",
                            "tokenCum": 616,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "For the purposes of this Act \u201cAdvisory Panel\u201d means a panel for the timebeing designated by the Secretary of State for those purposes.",
                                    "tokenCum": 647,
                                    "wordSum": 23,
                                    "tokenSum": 33,
                                    "tokenCount": 33
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "The Secretary of State may designate a panel for the purposes of this Actonly if the panel\u2019s functions consist of the consideration of claims which\u2014",
                                    "tokenCum": 677,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "are made in respect of objects, and",
                                            "tokenCum": 685,
                                            "wordSum": 7,
                                            "tokenSum": 11,
                                            "tokenCount": 11
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "relate to events occurring during the Nazi era.",
                                            "tokenCum": 696,
                                            "wordSum": 8,
                                            "tokenSum": 14,
                                            "tokenCount": 14
                                        }
                                    ],
                                    "wordSum": 40,
                                    "tokenSum": 57,
                                    "tokenCount": 31
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "\u201cNazi era\u201d means the period\u2014",
                                    "tokenCum": 705,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "beginning with 1 January 1933, and",
                                            "tokenCum": 717,
                                            "wordSum": 6,
                                            "tokenSum": 15,
                                            "tokenCount": 15
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "ending with 31 December 1945.",
                                            "tokenCum": 729,
                                            "wordSum": 5,
                                            "tokenSum": 15,
                                            "tokenCount": 15
                                        }
                                    ],
                                    "wordSum": 16,
                                    "tokenSum": 41,
                                    "tokenCount": 10
                                }
                            ],
                            "wordSum": 81,
                            "tokenSum": 139,
                            "tokenCount": 7
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 4",
                            "text": "Short title, extent, commencement and sunset",
                            "tokenCum": 740,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act may be cited as the Holocaust (Return of Cultural Objects) Act2009.",
                                    "tokenCum": 764,
                                    "wordSum": 13,
                                    "tokenSum": 26,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act extends to\u2014",
                                    "tokenCum": 769,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "England and Wales, and",
                                            "tokenCum": 774,
                                            "wordSum": 4,
                                            "tokenSum": 8,
                                            "tokenCount": 8
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "Scotland. ",
                                            "tokenCum": 777,
                                            "wordSum": 1,
                                            "tokenSum": 6,
                                            "tokenCount": 6
                                        }
                                    ],
                                    "wordSum": 9,
                                    "tokenSum": 21,
                                    "tokenCount": 6
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "The preceding sections of this Act come into force on such day as theSecretary of State may by order appoint.",
                                    "tokenCum": 800,
                                    "wordSum": 20,
                                    "tokenSum": 25,
                                    "tokenCount": 25
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "An order may make different provision for different purposes.",
                                    "tokenCum": 810,
                                    "wordSum": 9,
                                    "tokenSum": 12,
                                    "tokenCount": 12
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "Before appointing a day for the coming into force of the preceding sectionsof this Act so far as they relate to Scottish bodies the Secretary of Statemust consult the Scottish Ministers.",
                                    "tokenCum": 848,
                                    "wordSum": 31,
                                    "tokenSum": 40,
                                    "tokenCount": 40
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "\u201cScottish body\u201d has the meaning given by section 2(5).",
                                    "tokenCum": 865,
                                    "wordSum": 9,
                                    "tokenSum": 19,
                                    "tokenCount": 19
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(7)",
                                    "text": "This Act expires at the end of the period of 10 years beginning with theday on which it is passed.",
                                    "tokenCum": 890,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 890,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 20,
                                    "tokenSum": 30,
                                    "tokenCount": 26
                                }
                            ],
