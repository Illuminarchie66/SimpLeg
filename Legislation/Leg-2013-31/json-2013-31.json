            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2013/31/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "High Speed Rail (Preparation) Act 2013",
                    "tokenCum": 58,
                    "wordSum": 6,
                    "tokenSum": 15,
                    "tokenCount": 14
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2013 CHAPTER 31",
                    "tokenCum": 67,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision authorising expenditure in preparation for a highspeed railway transport network.",
                            "tokenCum": 88,
                            "wordSum": 15,
                            "tokenSum": 22,
                            "tokenCount": 22
                        }
                    ],
                    "wordSum": 18,
                    "tokenSum": 32,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[21st November 2013]",
                    "tokenCum": 99,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "BE IT ENACTED by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 157,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Preparatory expenditure",
                            "tokenCum": 163,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The Secretary of State may, with the approval of the Treasury, incurexpenditure in preparation for a high speed railway transport network.",
                                    "tokenCum": 195,
                                    "wordSum": 21,
                                    "tokenSum": 34,
                                    "tokenCount": 34
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "The network referred to in subsection (1) is a network which\u2014",
                                    "tokenCum": 210,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "involves the construction of railway lines connecting at least\u2014",
                                            "tokenCum": 220,
                                            "children": [
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * London, ",
                                                    "tokenCum": 225,
                                                    "wordSum": 2,
                                                    "tokenSum": 9,
                                                    "tokenCount": 9
                                                },
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * Birmingham, ",
                                                    "tokenCum": 231,
                                                    "wordSum": 2,
                                                    "tokenSum": 10,
                                                    "tokenCount": 10
                                                },
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * the East Midlands, ",
                                                    "tokenCum": 239,
                                                    "wordSum": 4,
                                                    "tokenSum": 12,
                                                    "tokenCount": 12
                                                },
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * Sheffield, ",
                                                    "tokenCum": 246,
                                                    "wordSum": 2,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                },
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * Leeds, and ",
                                                    "tokenCum": 253,
                                                    "wordSum": 3,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                },
                                                {
                                                    "type": "Bullet point",
                                                    "prefix": "*",
                                                    "text": "  * Manchester, and ",
                                                    "tokenCum": 259,
                                                    "wordSum": 3,
                                                    "tokenSum": 10,
                                                    "tokenCount": 10
                                                }
                                            ],
                                            "wordSum": 25,
                                            "tokenSum": 76,
                                            "tokenCount": 12
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "connects with the existing railway transport network as well as with suchother parts of the transport network (including roads, footpaths, cycleways,airports and light railways) as the Secretary of State considers appropriate.",
                                            "tokenCum": 304,
                                            "wordSum": 31,
                                            "tokenSum": 48,
                                            "tokenCount": 48
                                        }
                                    ],
                                    "wordSum": 67,
                                    "tokenSum": 141,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "The expenditure which may be incurred under subsection (1) includesexpenditure on\u2014",
                                    "tokenCum": 326,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "preparation for the construction of any railway line and any otherinfrastructure proposed to be included at any time in the network referred toin subsection (1), and",
                                            "tokenCum": 360,
                                            "wordSum": 26,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "preparation for the provision of services as part of that network.",
                                            "tokenCum": 373,
                                            "wordSum": 11,
                                            "tokenSum": 16,
                                            "tokenCount": 16
                                        }
                                    ],
                                    "wordSum": 48,
                                    "tokenSum": 77,
                                    "tokenCount": 23
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "The expenditure mentioned in subsection (3) includes expenditure incurred\u2014",
                                    "tokenCum": 392,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "on pre-construction activity (such as surveying and design),",
                                            "tokenCum": 406,
                                            "wordSum": 8,
                                            "tokenSum": 17,
                                            "tokenCount": 17
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "in acquiring property, and",
                                            "tokenCum": 412,
                                            "wordSum": 4,
                                            "tokenSum": 9,
                                            "tokenCount": 9
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "in providing compensation in respect of property likely to be affected.",
                                            "tokenCum": 425,
                                            "wordSum": 11,
                                            "tokenSum": 16,
                                            "tokenCount": 16
                                        }
                                    ],
                                    "wordSum": 32,
                                    "tokenSum": 63,
                                    "tokenCount": 20
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "In this Act, references to incurring expenditure include incurringliabilities.",
                                    "tokenCum": 444,
                                    "wordSum": 9,
                                    "tokenSum": 21,
                                    "tokenCount": 21
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "Expenditure incurred under this section is to be defrayed out of moneyprovided by Parliament.",
                                    "tokenCum": 466,
                                    "wordSum": 14,
                                    "tokenSum": 24,
                                    "tokenCount": 24
                                }
                            ],
                            "wordSum": 193,
                            "tokenSum": 367,
                            "tokenCount": 6
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Financial reports",
                            "tokenCum": 470,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The Secretary of State must prepare a report on expenditure under section 1in relation to each financial year.",
                                    "tokenCum": 493,
                                    "wordSum": 18,
                                    "tokenSum": 25,
                                    "tokenCount": 25
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Each report must contain details of\u2014",
                                    "tokenCum": 500,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "expenditure incurred under section 1 during the financial year to which thereport relates (with capital and resource expenditure specified separately inrespect of each head of expenditure referred to in section 1(4)(a) to (c));",
                                            "tokenCum": 555,
                                            "wordSum": 33,
                                            "tokenSum": 58,
                                            "tokenCount": 58
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "the extent to which expenditure incurred under section 1 during that yearrepresents an overspend or underspend as against the budget for suchexpenditure for the year;",
                                            "tokenCum": 595,
                                            "wordSum": 25,
                                            "tokenSum": 43,
                                            "tokenCount": 43
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(c)",
                                            "text": "the likely effect of any such overspend or underspend on a total budget of\u00a350.1 billion in 2011 prices (which includes construction and the cost ofrolling stock);",
                                            "tokenCum": 636,
                                            "wordSum": 26,
                                            "tokenSum": 44,
                                            "tokenCount": 44
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(d)",
                                            "text": "total expenditure incurred under section 1 up to the end of that year;",
                                            "tokenCum": 655,
                                            "wordSum": 13,
                                            "tokenSum": 22,
                                            "tokenCount": 22
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(e)",
                                            "text": "sums or assets received in that year in connection with expenditureincurred under section 1.",
                                            "tokenCum": 676,
                                            "wordSum": 14,
                                            "tokenSum": 24,
                                            "tokenCount": 24
                                        }
                                    ],
                                    "wordSum": 117,
                                    "tokenSum": 200,
                                    "tokenCount": 8
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "Each report must also contain an account of the vocational qualificationsgained during the financial year by individuals employed by persons appointedunder an enactment to carry out activities in connection with preparing for,and constructing, the network referred to in section 1(1).",
                                    "tokenCum": 732,
                                    "wordSum": 40,
                                    "tokenSum": 58,
                                    "tokenCount": 58
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "In this section, \u201cfinancial year\u201d means\u2014",
                                    "tokenCum": 744,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "the period beginning with the day on which this Act is passed and endingwith 31 March 2015, and",
                                            "tokenCum": 770,
                                            "wordSum": 18,
                                            "tokenSum": 29,
                                            "tokenCount": 29
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "each subsequent period of 12 months.",
                                            "tokenCum": 779,
                                            "wordSum": 6,
                                            "tokenSum": 12,
                                            "tokenCount": 12
                                        }
                                    ],
                                    "wordSum": 30,
                                    "tokenSum": 55,
                                    "tokenCount": 13
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "The Secretary of State must lay each report under this section beforeParliament as soon as is reasonably practicable after the end of the financialyear to which it relates.",
                                    "tokenCum": 815,
                                    "wordSum": 28,
                                    "tokenSum": 38,
                                    "tokenCount": 38
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "No report is required in relation to a financial year in which there isnothing to record under subsection (2)(a) and (e).",
                                    "tokenCum": 844,
                                    "wordSum": 21,
                                    "tokenSum": 31,
                                    "tokenCount": 31
                                }
                            ],
                            "wordSum": 256,
                            "tokenSum": 412,
                            "tokenCount": 4
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Extent, commencement and short title",
                            "tokenCum": 853,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act extends to England and Wales and to Scotland.",
                                    "tokenCum": 864,
                                    "wordSum": 10,
                                    "tokenSum": 13,
                                    "tokenCount": 13
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "This Act comes into force on the day on which it is passed.",
                                    "tokenCum": 878,
                                    "wordSum": 13,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This Act may be cited as the High Speed Rail (Preparation) Act 2013.",
                                    "tokenCum": 901,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 901,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 13,
                                    "tokenSum": 28,
                                    "tokenCount": 24
                                }
                            ],
                            "wordSum": 41,
                            "tokenSum": 67,
                            "tokenCount": 9
                        }
                    ],
                    "wordSum": 526,
                    "tokenSum": 905,
                    "tokenCount": 58
                }
            ],
