            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2002/31/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Mobile Telephones (Re-programming) Act 2002",
                    "tokenCum": 59,
                    "wordSum": 5,
                    "tokenSum": 16,
                    "tokenCount": 15
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2002 CHAPTER 31",
                    "tokenCum": 68,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to create offences in respect of unique electronic equipmentidentifiers of mobile wireless communications devices.",
                            "tokenCum": 89,
                            "wordSum": 16,
                            "tokenSum": 22,
                            "tokenCount": 22
                        }
                    ],
                    "wordSum": 19,
                    "tokenSum": 32,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[24th July 2002]",
                    "tokenCum": 100,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 157,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Re-programming mobile telephone etc.",
                            "tokenCum": 165,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "A person commits an offence if\u2014",
                                    "tokenCum": 173,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "he changes a unique device identifier, or",
                                            "tokenCum": 181,
                                            "wordSum": 7,
                                            "tokenSum": 11,
                                            "tokenCount": 11
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "he interferes with the operation of a unique device identifier.",
                                            "tokenCum": 194,
                                            "wordSum": 10,
                                            "tokenSum": 16,
                                            "tokenCount": 16
                                        }
                                    ],
                                    "wordSum": 23,
                                    "tokenSum": 37,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "A unique device identifier is an electronic equipment identifier which isunique to a mobile wireless communications device.",
                                    "tokenCum": 214,
                                    "wordSum": 17,
                                    "tokenSum": 22,
                                    "tokenCount": 22
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "But a person does not commit an offence under this section if\u2014",
                                    "tokenCum": 228,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "he is the manufacturer of the device, or",
                                            "tokenCum": 238,
                                            "wordSum": 8,
                                            "tokenSum": 13,
                                            "tokenCount": 13
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "he does the act mentioned in subsection (1) with the written consent of themanufacturer of the device.",
                                            "tokenCum": 262,
                                            "wordSum": 17,
                                            "tokenSum": 27,
                                            "tokenCount": 27
                                        }
                                    ],
                                    "wordSum": 37,
                                    "tokenSum": 56,
                                    "tokenCount": 15
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "A person guilty of an offence under this section is liable\u2014",
                                    "tokenCum": 276,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "on summary conviction, to imprisonment for a term not exceeding 6 months orto a fine not exceeding the statutory maximum or to both, or",
                                            "tokenCum": 310,
                                            "wordSum": 24,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "on conviction on indictment, to imprisonment for a term not exceeding 5years or to a fine or to both.",
                                            "tokenCum": 339,
                                            "wordSum": 19,
                                            "tokenSum": 32,
                                            "tokenCount": 32
                                        }
                                    ],
                                    "wordSum": 54,
                                    "tokenSum": 85,
                                    "tokenCount": 15
                                }
                            ],
                            "wordSum": 135,
                            "tokenSum": 209,
                            "tokenCount": 8
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Possession or supply of anything for re-programming purposes",
                            "tokenCum": 350,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "A person commits an offence if\u2014",
                                    "tokenCum": 358,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "he has in his custody or under his control anything which may be used forthe purpose of changing or interfering with the operation of a unique deviceidentifier, and",
                                            "tokenCum": 392,
                                            "wordSum": 28,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "he intends to use the thing unlawfully for that purpose or to allow it tobe used unlawfully for that purpose.",
                                            "tokenCum": 419,
                                            "wordSum": 20,
                                            "tokenSum": 30,
                                            "tokenCount": 30
                                        }
                                    ],
                                    "wordSum": 54,
                                    "tokenSum": 77,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "A person commits an offence if\u2014",
                                    "tokenCum": 427,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "he supplies anything which may be used for the purpose of changing orinterfering with the operation of a unique device identifier, and",
                                            "tokenCum": 453,
                                            "wordSum": 22,
                                            "tokenSum": 29,
                                            "tokenCount": 29
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "he knows or believes that the person to whom the thing is supplied intendsto use it unlawfully for that purpose or to allow it to be used unlawfully forthat purpose.",
                                            "tokenCum": 491,
                                            "wordSum": 30,
                                            "tokenSum": 41,
                                            "tokenCount": 41
                                        }
                                    ],
                                    "wordSum": 58,
                                    "tokenSum": 80,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "A person commits an offence if\u2014",
                                    "tokenCum": 499,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "he offers to supply anything which may be used for the purpose of changingor interfering with the operation of a unique device identifier, and",
                                            "tokenCum": 527,
                                            "wordSum": 24,
                                            "tokenSum": 31,
                                            "tokenCount": 31
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "he knows or believes that the person to whom the thing is offered intendsif it is supplied to him to use it unlawfully for that purpose or to allow itto be used unlawfully for that purpose.",
                                            "tokenCum": 572,
                                            "wordSum": 36,
                                            "tokenSum": 48,
                                            "tokenCount": 48
                                        }
                                    ],
                                    "wordSum": 66,
                                    "tokenSum": 89,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "A unique device identifier is an electronic equipment identifier which isunique to a mobile wireless communications device.",
                                    "tokenCum": 592,
                                    "wordSum": 17,
                                    "tokenSum": 22,
                                    "tokenCount": 22
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "A thing is used by a person unlawfully for a purpose if in using it forthat purpose he commits an offence under section 1.",
                                    "tokenCum": 622,
                                    "wordSum": 24,
                                    "tokenSum": 32,
                                    "tokenCount": 32
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "A person guilty of an offence under this section is liable\u2014",
                                    "tokenCum": 636,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "on summary conviction, to imprisonment for a term not exceeding 6 months orto a fine not exceeding the statutory maximum or to both, or",
                                            "tokenCum": 670,
                                            "wordSum": 24,
                                            "tokenSum": 37,
                                            "tokenCount": 37
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "on conviction on indictment, to imprisonment for a term not exceeding 5years or to a fine or to both.",
                                            "tokenCum": 699,
                                            "wordSum": 19,
                                            "tokenSum": 32,
                                            "tokenCount": 32
                                        }
                                    ],
                                    "wordSum": 54,
                                    "tokenSum": 85,
                                    "tokenCount": 15
                                }
                            ],
                            "wordSum": 281,
                            "tokenSum": 397,
                            "tokenCount": 11
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Citation etc.",
                            "tokenCum": 703,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act may be cited as the Mobile Telephones (Re-programming) Act 2002.",
                                    "tokenCum": 727,
                                    "wordSum": 12,
                                    "tokenSum": 26,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Sections 1 and 2 come into force in accordance with provision made by theSecretary of State by order made by statutory instrument.",
                                    "tokenCum": 758,
                                    "wordSum": 22,
                                    "tokenSum": 33,
                                    "tokenCount": 33
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This Act extends to Northern Ireland.",
                                    "tokenCum": 765,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 765,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 6,
                                    "tokenSum": 12,
                                    "tokenCount": 8
                                }
                            ],
                            "wordSum": 42,
                            "tokenSum": 76,
                            "tokenCount": 4
                        }
                    ],
                    "wordSum": 494,
                    "tokenSum": 740,
                    "tokenCount": 57
                }
            ],
