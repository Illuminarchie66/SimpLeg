        {
            "type": "Text",
            "prefix": "",
            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2012/22/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Police (Complaints and Conduct) Act 2012",
                    "tokenCum": 59,
                    "wordSum": 6,
                    "tokenSum": 16,
                    "tokenCount": 15
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2012 CHAPTER 22",
                    "tokenCum": 68,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision about interviews held during certain investigationsunder Schedule 3 to the Police Reform Act 2002; and about the application ofPart 2 of that Act to matters occurring before 1 April 2004.",
                            "tokenCum": 122,
                            "wordSum": 35,
                            "tokenSum": 55,
                            "tokenCount": 55
                        }
                    ],
                    "wordSum": 38,
                    "tokenSum": 65,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[19th December 2012]",
                    "tokenCum": 133,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 190,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Interviews of serving officers during investigations",
                            "tokenCum": 198,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The Police Reform Act 2002 is amended as follows.",
                                    "tokenCum": 213,
                                    "wordSum": 9,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "In Schedule 3 (handling of complaints and conduct matters etc) afterparagraph 19E insert\u2014",
                                    "tokenCum": 237,
                                    "wordSum": 13,
                                    "tokenSum": 26,
                                    "tokenCount": 26
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "###",
                                    "text": "\u201cInterview of persons serving with the police etc during certaininvestigations",
                                    "tokenCum": 252,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "19F(1)This paragraph applies to an investigation of a complaint, recordableconduct matter or DSI matter which\u2014",
                                            "tokenCum": 281,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "is carried out by the appropriate authority under the management of theCommission, or",
                                                    "tokenCum": 297,
                                                    "wordSum": 13,
                                                    "tokenSum": 20,
                                                    "tokenCount": 20
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "is carried out by the Commission itself.",
                                                    "tokenCum": 305,
                                                    "wordSum": 7,
                                                    "tokenSum": 12,
                                                    "tokenCount": 12
                                                }
                                            ],
                                            "wordSum": 35,
                                            "tokenSum": 64,
                                            "tokenCount": 31
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "The Secretary of State may by regulations make provision as to theprocedure to be followed in connection with an interview which\u2014",
                                            "tokenCum": 330,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "is held with a serving officer during the course of the investigation bythe person investigating the complaint or matter, and",
                                                    "tokenCum": 354,
                                                    "wordSum": 20,
                                                    "tokenSum": 28,
                                                    "tokenCount": 28
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "is not within paragraph 19D(1).",
                                                    "tokenCum": 365,
                                                    "wordSum": 5,
                                                    "tokenSum": 15,
                                                    "tokenCount": 15
                                                }
                                            ],
                                            "wordSum": 46,
                                            "tokenSum": 71,
                                            "tokenCount": 27
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "Regulations under sub-paragraph (2) may in particular make provision\u2014",
                                            "tokenCum": 380,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "requiring a serving officer to attend an interview,",
                                                    "tokenCum": 389,
                                                    "wordSum": 8,
                                                    "tokenSum": 13,
                                                    "tokenCount": 13
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "for determining how the time at which an interview is to be held is to beagreed or decided,",
                                                    "tokenCum": 412,
                                                    "wordSum": 18,
                                                    "tokenSum": 27,
                                                    "tokenCount": 27
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "about the information that must be provided to a serving officer beinginterviewed,",
                                                    "tokenCum": 428,
                                                    "wordSum": 12,
                                                    "tokenSum": 20,
                                                    "tokenCount": 20
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(d)",
                                                    "text": "for enabling a serving officer to be accompanied at the interview by aperson of a prescribed description.",
                                                    "tokenCum": 449,
                                                    "wordSum": 17,
                                                    "tokenSum": 25,
                                                    "tokenCount": 25
                                                }
                                            ],
                                            "wordSum": 64,
                                            "tokenSum": 103,
                                            "tokenCount": 17
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(4)",
                                            "text": "\u201cServing officer\u201d means a person who\u2014",
                                            "tokenCum": 459,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "is serving with the police, or",
                                                    "tokenCum": 466,
                                                    "wordSum": 6,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "is serving with an additional police body.",
                                                    "tokenCum": 474,
                                                    "wordSum": 7,
                                                    "tokenSum": 12,
                                                    "tokenCount": 12
                                                }
                                            ],
                                            "wordSum": 19,
                                            "tokenSum": 36,
                                            "tokenCount": 12
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(5)",
                                            "text": "A person is serving with an additional police body if the person is amember of, or is employed or otherwise engaged for the purposes of, that body(subject to sub-paragraph (6)).",
                                            "tokenCum": 513,
                                            "wordSum": 30,
                                            "tokenSum": 42,
                                            "tokenCount": 42
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(6)",
                                            "text": "The Secretary of State may by regulations provide, in relation to anadditional police body, that a person is serving with that body only if theperson\u2014",
                                            "tokenCum": 545,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "is a member of, or is employed or otherwise engaged for the purposes of,that body, and",
                                                    "tokenCum": 565,
                                                    "wordSum": 16,
                                                    "tokenSum": 24,
                                                    "tokenCount": 24
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "is of a prescribed description.",
                                                    "tokenCum": 572,
                                                    "wordSum": 5,
                                                    "tokenSum": 11,
                                                    "tokenCount": 11
                                                }
                                            ],
                                            "wordSum": 46,
                                            "tokenSum": 70,
                                            "tokenCount": 34
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(7)",
                                            "text": "An \u201cadditional police body\u201d means\u2014",
                                            "tokenCum": 581,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "a body of constables which is maintained by an authority other than a localpolicing body and is prescribed in regulations made by the Secretary of Stateunder this sub-paragraph, or",
                                                    "tokenCum": 619,
                                                    "wordSum": 29,
                                                    "tokenSum": 42,
                                                    "tokenCount": 42
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "a body required by section 26A or 26B to enter into an agreement with theCommission.",
                                                    "tokenCum": 643,
                                                    "wordSum": 15,
                                                    "tokenSum": 28,
                                                    "tokenCount": 28
                                                }
                                            ],
                                            "wordSum": 49,
                                            "tokenSum": 82,
                                            "tokenCount": 11
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(8)",
                                            "text": "The Secretary of State must obtain the consent of the Northern IrelandAssembly before making provision in regulations under this paragraph whichwould be within the legislative competence of the Northern Ireland Assembly.",
                                            "tokenCum": 681,
                                            "wordSum": 31,
                                            "tokenSum": 41,
                                            "tokenCount": 41
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(9)",
                                            "text": "But consent under sub-paragraph (8) is not required in relation to aprovision if\u2014",
                                            "tokenCum": 700,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "a Bill for an Act of the Northern Ireland Assembly containing the provisionwould require the consent of the Secretary of State under section 8 of theNorthern Ireland Act 1998, and",
                                                    "tokenCum": 741,
                                                    "wordSum": 30,
                                                    "tokenSum": 45,
                                                    "tokenCount": 45
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "the provision does not affect, other than incidentally, a transferredmatter (within the meaning of that Act).",
                                                    "tokenCum": 765,
                                                    "wordSum": 16,
                                                    "tokenSum": 28,
                                                    "tokenCount": 28
                                                }
                                            ],
                                            "wordSum": 59,
                                            "tokenSum": 95,
                                            "tokenCount": 21
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(10)",
                                            "text": "Nothing in this paragraph prevents or restricts the holding of interviewsto which regulations under this paragraph do not apply during the course ofany investigation under this Schedule.\u201d",
                                            "tokenCum": 798,
                                            "wordSum": 27,
                                            "tokenSum": 36,
                                            "tokenCount": 36
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "In paragraph 19D(2) of Schedule 3 (interview in certain cases of policeofficer or special constable whose conduct is being investigated) beforeparagraph (a) insert\u2014",
                                            "tokenCum": 841,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "\u201c(za)",
                                                    "text": "requiring the person concerned to attend an interview,\u201d.",
                                                    "tokenCum": 851,
                                                    "wordSum": 8,
                                                    "tokenSum": 14,
                                                    "tokenCount": 14
                                                }
                                            ],
                                            "wordSum": 31,
                                            "tokenSum": 60,
                                            "tokenCount": 45
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(4)",
                                            "text": "In section 108(7) (provisions of Act which extend to the United Kingdom)before paragraph (a) insert\u2014",
                                            "tokenCum": 880,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "\u201c(za)",
                                                    "text": "the provision contained in paragraph 19F of Schedule 3 (and anyinterpretative or other supplementary provision as it has effect for thepurposes of that provision);\u201d.",
                                                    "tokenCum": 919,
                                                    "wordSum": 24,
                                                    "tokenSum": 43,
                                                    "tokenCount": 43
                                                }
                                            ],
                                            "wordSum": 39,
                                            "tokenSum": 75,
                                            "tokenCount": 31
                                        }
                                    ],
                                    "wordSum": 486,
                                    "tokenSum": 792,
                                    "tokenCount": 16
                                }
                            ],
                            "wordSum": 514,
                            "tokenSum": 844,
                            "tokenCount": 8
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Application of Part 2 of the Police Reform Act 2002 to old cases",
                            "tokenCum": 937,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "The Police Reform Act 2002 is amended as follows.",
                                    "tokenCum": 952,
                                    "wordSum": 9,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "After section 28 insert\u2014",
                                    "tokenCum": 959,
                                    "wordSum": 4,
                                    "tokenSum": 9,
                                    "tokenCount": 9
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "###",
                                    "text": "\u201c28AApplication of Part 2 to old cases",
                                    "tokenCum": 971,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The Commission may, if it considers that there are exceptionalcircumstances\u2014",
                                            "tokenCum": 989,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "direct that a relevant transitional provision does not apply in relation toa pre-commencement matter, and",
                                                    "tokenCum": 1011,
                                                    "wordSum": 15,
                                                    "tokenSum": 26,
                                                    "tokenCount": 26
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "direct the appropriate authority to record the matter under this Part.",
                                                    "tokenCum": 1023,
                                                    "wordSum": 11,
                                                    "tokenSum": 16,
                                                    "tokenCount": 16
                                                }
                                            ],
                                            "wordSum": 36,
                                            "tokenSum": 63,
                                            "tokenCount": 20
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "A \u201cpre-commencement matter\u201d means a matter which\u2014",
                                            "tokenCum": 1037,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "is a complaint or a conduct matter,",
                                                    "tokenCum": 1046,
                                                    "wordSum": 7,
                                                    "tokenSum": 13,
                                                    "tokenCount": 13
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "relates to conduct which took place, or circumstances which occurred,before 1 April 2004, and",
                                                    "tokenCum": 1070,
                                                    "wordSum": 14,
                                                    "tokenSum": 28,
                                                    "tokenCount": 28
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "(apart from this section) is prevented by a relevant transitional provisionfrom being recorded under this Part as a complaint or a conduct matter.",
                                                    "tokenCum": 1101,
                                                    "wordSum": 23,
                                                    "tokenSum": 35,
                                                    "tokenCount": 35
                                                }
                                            ],
                                            "wordSum": 51,
                                            "tokenSum": 93,
                                            "tokenCount": 16
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "\u201cRelevant transitional provision\u201d means article 2, 3 or 4 of theIndependent Police Complaints Commission (Transitional Provisions) Order 2004([S.I. 2004/671](/id/uksi/2004/671?view=plain \"Go to item of legislation\")).",
                                            "tokenCum": 1182,
                                            "wordSum": 24,
                                            "tokenSum": 84,
                                            "tokenCount": 84
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(4)",
                                            "text": "The Commission may, if it considers that there are exceptionalcircumstances, direct that a matter to which subsection (5) applies should betreated as a conduct matter or a DSI matter.",
                                            "tokenCum": 1225,
                                            "wordSum": 29,
                                            "tokenSum": 46,
                                            "tokenCount": 46
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(5)",
                                            "text": "This subsection applies to a matter which\u2014",
                                            "tokenCum": 1234,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "relates to conduct which took place, or circumstances which occurred,before 1 April 2004,",
                                                    "tokenCum": 1257,
                                                    "wordSum": 13,
                                                    "tokenSum": 27,
                                                    "tokenCount": 27
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "has been the subject of a relevant complaint, and",
                                                    "tokenCum": 1268,
                                                    "wordSum": 9,
                                                    "tokenSum": 15,
                                                    "tokenCount": 15
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "would be a conduct matter or a DSI matter if it had not been the subject ofa relevant complaint.",
                                                    "tokenCum": 1291,
                                                    "wordSum": 19,
                                                    "tokenSum": 27,
                                                    "tokenCount": 27
                                                }
                                            ],
                                            "wordSum": 48,
                                            "tokenSum": 81,
                                            "tokenCount": 11
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(6)",
                                            "text": "\u201cRelevant complaint\u201d means\u2014",
                                            "tokenCum": 1299,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "a complaint made before 1 April 2004, or",
                                                    "tokenCum": 1314,
                                                    "wordSum": 8,
                                                    "tokenSum": 19,
                                                    "tokenCount": 19
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "a complaint made on or after that date to which a relevant transitionalprovision applied.",
                                                    "tokenCum": 1333,
                                                    "wordSum": 14,
                                                    "tokenSum": 23,
                                                    "tokenCount": 23
                                                }
                                            ],
                                            "wordSum": 25,
                                            "tokenSum": 53,
                                            "tokenCount": 10
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(7)",
                                            "text": "Where under subsection (1)(a) the Commission directs that a relevanttransitional provision does not apply in relation to a pre-commencementmatter, that provision does not apply in relation to that matter.",
                                            "tokenCum": 1378,
                                            "wordSum": 29,
                                            "tokenSum": 48,
                                            "tokenCount": 48
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(8)",
                                            "text": "The appropriate authority must comply with a direction under subsection(1)(b).",
                                            "tokenCum": 1395,
                                            "wordSum": 10,
                                            "tokenSum": 20,
                                            "tokenCount": 20
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(9)",
                                            "text": "Where under subsection (4) the Commission directs that a matter should betreated as a conduct matter or a DSI matter, the matter is (subject to anyregulations under subsection (10)) to be treated as a conduct matter or a DSImatter for the purposes of this Part (including subsections (1) and (2)) andany provision made under it.",
                                            "tokenCum": 1475,
                                            "wordSum": 55,
                                            "tokenSum": 83,
                                            "tokenCount": 83
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(10)",
                                            "text": "The Secretary of State may by regulations provide for this Part or anyprovision made under it to apply with such modifications as the Secretary ofState thinks fit to a matter in relation to which the Commission has made adirection under subsection (1) or (4).\u201d",
                                            "tokenCum": 1530,
                                            "wordSum": 44,
                                            "tokenSum": 58,
                                            "tokenCount": 58
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "In section 12 (definitions of \u201cconduct matter\u201d and \u201cDSI matter\u201d)\u2014",
                                            "tokenCum": 1552,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "in subsection (2), after \u201cthis section,\u201d insert \u201csection 28A and anyregulations made under it,\u201d, and",
                                                    "tokenCum": 1580,
                                                    "wordSum": 15,
                                                    "tokenSum": 32,
                                                    "tokenCount": 32
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "in subsection (2A), after \u201cmeans\u201d insert \u201c(subject to section 28A and anyregulations made under it)\u201d.",
                                                    "tokenCum": 1611,
                                                    "wordSum": 15,
                                                    "tokenSum": 35,
                                                    "tokenCount": 35
                                                }
                                            ],
                                            "wordSum": 40,
                                            "tokenSum": 92,
                                            "tokenCount": 24
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(4)",
                                            "text": "In section 29(1), in the definition of \u201crecordable conduct matter\u201d, afterparagraph (a) (and the \u201cor\u201d which follows) insert\u2014",
                                            "tokenCum": 1645,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "\u201c(aa)",
                                                    "text": "a conduct matter that is required to be recorded by the appropriateauthority under section 28A(8) or has been so recorded;\u201d.",
                                                    "tokenCum": 1675,
                                                    "wordSum": 20,
                                                    "tokenSum": 34,
                                                    "tokenCount": 34
                                                }
                                            ],
                                            "wordSum": 38,
                                            "tokenSum": 71,
                                            "tokenCount": 36
                                        }
                                    ],
                                    "wordSum": 436,
                                    "tokenSum": 806,
                                    "tokenCount": 13
                                }
                            ],
                            "wordSum": 462,
                            "tokenSum": 851,
                            "tokenCount": 18
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Extent, commencement and short title",
                            "tokenCum": 1684,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "Section 1 (except for subsection (3)) and this section extend to Englandand Wales, Scotland and Northern Ireland.",
                                    "tokenCum": 1709,
                                    "wordSum": 17,
                                    "tokenSum": 27,
                                    "tokenCount": 27
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Sections 1(3) and 2 extend to England and Wales only.",
                                    "tokenCum": 1726,
                                    "wordSum": 10,
                                    "tokenSum": 19,
                                    "tokenCount": 19
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This Act comes into force on the day on which it is passed.",
                                    "tokenCum": 1740,
                                    "wordSum": 13,
                                    "tokenSum": 16,
                                    "tokenCount": 16
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "This Act may be cited as the Police (Complaints and Conduct) Act 2012.",
                                    "tokenCum": 1764,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "",
                                            "tokenCum": 1764,
                                            "wordSum": 0,
                                            "tokenSum": 3,
                                            "tokenCount": 3
                                        }
                                    ],
                                    "wordSum": 13,
                                    "tokenSum": 29,
                                    "tokenCount": 25
                                }
                            ],
                            "wordSum": 58,
                            "tokenSum": 101,
                            "tokenCount": 9
                        }
                    ],
