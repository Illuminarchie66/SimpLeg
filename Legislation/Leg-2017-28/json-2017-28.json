            "text": "[![LegislationCrest](/images/crests/ukpga.gif)](/ukpga/2017/28/introduction/enacted?view=plain)",
            "tokenCum": 44,
            "children": [
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "Farriers (Registration) Act 2017",
                    "tokenCum": 56,
                    "wordSum": 4,
                    "tokenSum": 13,
                    "tokenCount": 12
                },
                {
                    "type": "Heading",
                    "prefix": "#",
                    "text": "2017 CHAPTER 28",
                    "tokenCum": 65,
                    "children": [
                        {
                            "type": "Text",
                            "prefix": "",
                            "text": "An Act to make provision about the constitution of the Farriers RegistrationCouncil and its committees.",
                            "tokenCum": 88,
                            "wordSum": 15,
                            "tokenSum": 24,
                            "tokenCount": 24
                        }
                    ],
                    "wordSum": 18,
                    "tokenSum": 34,
                    "tokenCount": 9
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "[27th April 2017]",
                    "tokenCum": 99,
                    "wordSum": 3,
                    "tokenSum": 12,
                    "tokenCount": 11
                },
                {
                    "type": "Text",
                    "prefix": "",
                    "text": "Be it enacted by the Queen\u2019s most Excellent Majesty, by and with the adviceand consent of the Lords Spiritual and Temporal, and Commons, in this presentParliament assembled, and by the authority of the same, as follows:\u2014",
                    "tokenCum": 156,
                    "children": [
                        {
                            "type": "Heading",
                            "prefix": "## 1",
                            "text": "Constitution of Farriers Registration Council and its committees",
                            "tokenCum": 168,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "The Schedule amends the Farriers (Registration) Act 1975 to make changes tothe constitution of the Farriers Registration Council and its committees.",
                                    "tokenCum": 205,
                                    "wordSum": 21,
                                    "tokenSum": 39,
                                    "tokenCount": 39
                                }
                            ],
                            "wordSum": 29,
                            "tokenSum": 52,
                            "tokenCount": 12
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 2",
                            "text": "Power to make further changes to constitution of Council and itscommittees",
                            "tokenCum": 219,
                            "children": [
                                {
                                    "type": "Text",
                                    "prefix": "",
                                    "text": "In the Farriers (Registration) Act 1975, after section 17 insert\u2014",
                                    "tokenCum": 241,
                                    "wordSum": 10,
                                    "tokenSum": 24,
                                    "tokenCount": 24
                                },
                                {
                                    "type": "Heading",
                                    "prefix": "###",
                                    "text": "\u201c17APower to change constitution of Council and its committees",
                                    "tokenCum": 256,
                                    "children": [
                                        {
                                            "type": "Digit point",
                                            "prefix": "(1)",
                                            "text": "The Secretary of State may by regulations amend or replace\u2014",
                                            "tokenCum": 269,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "Part 1 of Schedule 1 (constitution of the Council);",
                                                    "tokenCum": 284,
                                                    "wordSum": 9,
                                                    "tokenSum": 19,
                                                    "tokenCount": 19
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "Schedule 2 (constitution of the Investigating Committee);",
                                                    "tokenCum": 297,
                                                    "wordSum": 7,
                                                    "tokenSum": 17,
                                                    "tokenCount": 17
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(c)",
                                                    "text": "Part 1 of Schedule 3 (constitution of the Disciplinary Committee).",
                                                    "tokenCum": 315,
                                                    "wordSum": 10,
                                                    "tokenSum": 22,
                                                    "tokenCount": 22
                                                }
                                            ],
                                            "wordSum": 36,
                                            "tokenSum": 74,
                                            "tokenCount": 15
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(2)",
                                            "text": "Before making regulations under this section, the Secretary of State mustobtain the consent of\u2014",
                                            "tokenCum": 334,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "the Scottish Ministers, and",
                                                    "tokenCum": 340,
                                                    "wordSum": 4,
                                                    "tokenSum": 10,
                                                    "tokenCount": 10
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "the Welsh Ministers.",
                                                    "tokenCum": 346,
                                                    "wordSum": 3,
                                                    "tokenSum": 10,
                                                    "tokenCount": 10
                                                }
                                            ],
                                            "wordSum": 21,
                                            "tokenSum": 42,
                                            "tokenCount": 21
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(3)",
                                            "text": "Before making regulations under this section, the Secretary of State mustconsult\u2014",
                                            "tokenCum": 362,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "the Council, and",
                                                    "tokenCum": 366,
                                                    "wordSum": 3,
                                                    "tokenSum": 8,
                                                    "tokenCount": 8
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "any other persons the Secretary of State considers appropriate.",
                                                    "tokenCum": 377,
                                                    "wordSum": 9,
                                                    "tokenSum": 15,
                                                    "tokenCount": 15
                                                }
                                            ],
                                            "wordSum": 23,
                                            "tokenSum": 42,
                                            "tokenCount": 18
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(4)",
                                            "text": "Regulations under this section\u2014",
                                            "tokenCum": 383,
                                            "children": [
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(a)",
                                                    "text": "are to be made by statutory instrument;",
                                                    "tokenCum": 393,
                                                    "wordSum": 7,
                                                    "tokenSum": 14,
                                                    "tokenCount": 14
                                                },
                                                {
                                                    "type": "Alpha point",
                                                    "prefix": "(b)",
                                                    "text": "may include incidental, supplementary, consequential, transitional,transitory or saving provision.",
                                                    "tokenCum": 414,
                                                    "wordSum": 9,
                                                    "tokenSum": 25,
                                                    "tokenCount": 25
                                                }
                                            ],
                                            "wordSum": 20,
                                            "tokenSum": 48,
                                            "tokenCount": 8
                                        },
                                        {
                                            "type": "Digit point",
                                            "prefix": "(5)",
                                            "text": "A statutory instrument containing regulations under this section is subjectto annulment in pursuance of a resolution of either House of Parliament.\u201d",
                                            "tokenCum": 444,
                                            "wordSum": 21,
                                            "tokenSum": 33,
                                            "tokenCount": 33
                                        }
                                    ],
                                    "wordSum": 130,
                                    "tokenSum": 256,
                                    "tokenCount": 16
                                }
                            ],
                            "wordSum": 151,
                            "tokenSum": 295,
                            "tokenCount": 14
                        },
                        {
                            "type": "Heading",
                            "prefix": "## 3",
                            "text": "Extent, commencement and short title",
                            "tokenCum": 453,
                            "children": [
                                {
                                    "type": "Digit point",
                                    "prefix": "(1)",
                                    "text": "This Act extends to England and Wales and Scotland.",
                                    "tokenCum": 463,
                                    "wordSum": 9,
                                    "tokenSum": 12,
                                    "tokenCount": 12
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(2)",
                                    "text": "Sections 1 and 2 and the Schedule come into force on such day as theSecretary of State may by regulations appoint.",
                                    "tokenCum": 492,
                                    "wordSum": 21,
                                    "tokenSum": 31,
                                    "tokenCount": 31
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(3)",
                                    "text": "This section comes into force on the day on which this Act is passed.",
                                    "tokenCum": 507,
                                    "wordSum": 14,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(4)",
                                    "text": "Regulations under subsection (2)\u2014",
                                    "tokenCum": 516,
                                    "children": [
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(a)",
                                            "text": "may appoint different days for different purposes;",
                                            "tokenCum": 524,
                                            "wordSum": 7,
                                            "tokenSum": 11,
                                            "tokenCount": 11
                                        },
                                        {
                                            "type": "Alpha point",
                                            "prefix": "(b)",
                                            "text": "may make transitional, transitory or saving provision.",
                                            "tokenCum": 535,
                                            "wordSum": 7,
                                            "tokenSum": 14,
                                            "tokenCount": 14
                                        }
                                    ],
                                    "wordSum": 18,
                                    "tokenSum": 36,
                                    "tokenCount": 10
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(5)",
                                    "text": "Regulations under this section are to be made by statutory instrument.",
                                    "tokenCum": 550,
                                    "wordSum": 11,
                                    "tokenSum": 17,
                                    "tokenCount": 17
                                },
                                {
                                    "type": "Digit point",
                                    "prefix": "(6)",
                                    "text": "This Act may be cited as the Farriers (Registration) Act 2017.",
                                    "tokenCum": 571,
                                    "children": [
                                        {
                                            "type": "Text",
                                            "prefix": "",
                                            "text": "Section 1",
                                            "tokenCum": 574,
                                            "wordSum": 2,
                                            "tokenSum": 6,
                                            "tokenCount": 6
                                        }
                                    ],
                                    "wordSum": 13,
                                    "tokenSum": 29,
                                    "tokenCount": 22
                                }
                            ],
                            "wordSum": 91,
                            "tokenSum": 152,
                            "tokenCount": 9
                        }
                    ],
                    "wordSum": 307,
                    "tokenSum": 557,
                    "tokenCount": 57
                }
            ],
